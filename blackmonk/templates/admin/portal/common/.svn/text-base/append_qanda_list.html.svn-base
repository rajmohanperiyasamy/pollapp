{% load i18n %}
<li class="cat-list-row" id="div_{{qanda.id}}">
	<div class="cat-list-row-inner group">
    	<span class="checkbox-wrap">
			<span class="cform-checkbox-container"><input type="checkbox" id="qanda" value="{{qanda.id}}" name="qanda" class="check-overflow"><span class="cform-checkbox-element"></span></span>
		</span>
        <div class="cat-list-column cat-name"><h3>{{qanda|truncatechars:30}}</h3></div>
        <div class="cat-list-column action-buttons">
        	<div class="group">
            	<!--a title="{% trans 'View Stats' %}" class="tttxt-n" href="#"><span class="icon icon-stats"></span></a-->
                <a href="{% url 'admin_portal_sweepstakes_qanda_update' %}?id={{qanda.id}}" title="{% trans 'Update Q&A' %}" class="tttxt-n bm-add-gadget" id="edit_link{{qanda.id}}"><span class="icon icon-edit"></span></a>
                <a title="{% trans 'Delete' %}" class="tttxt-n" href="javascript:delete_qanda({{qanda.id}})"><span class="icon icon-delete"></span></a>
            </div>                                                            
		</div>
	</div>
</li>    
