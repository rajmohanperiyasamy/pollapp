<script type="text/javascript">
$(document).ready(function(){
	$('.expand_span').live("click",function(event){
		event.preventDefault();
		if($(this).hasClass('nav-collaps')){
			$('.left-nav-container').removeClass('selected-child');
			$(this).parent('a').parent('li').addClass('selected-child');
			$('.nav-expand').addClass('nav-collaps').removeClass('nav-expand');
			$(this).removeClass('nav-collaps').addClass('nav-expand');	
		}else{
			$(this).parent('a').parent('li').toggleClass('selected-child');
			$(this).toggleClass('nav-collaps').toggleClass('nav-expand');
		}
	});
});
</script>
