{% load i18n %}
<div id="ajax_content_loading_seo" style="display:none;padding-top:150px;text-align:center;">
	<img src="{{STATIC_URL}}ui/images/global/loading-s.gif"><br><p>{% trans 'Saving Video SEO' %}</p>
</div>
<div class="main_col" id="ajax_content_seo">

	<div id="add-new-category" class="modal-form">
    	<div class="fields hform">
    		<div id="meta-tags" class="field-group">
    			<div class="fields-head">
                	<h3>Video Details</h3>
				</div>  
    			<div class="field type-text">
                </div>  
	        	<div class="field text-field">	
		           	<div class="label"><label>{% trans 'Title' %}</label>
						{{video.title}}
					</div>	
			            <div class="value">{% if video.featured %}</div>
						<h3>{% trans "Featured Video" %} <h3>	
						{% else %}
						<h3>{% trans "Video is not featured " %}<h3>
							{% endif %}
		            </div>
		           	<div >	
		                <div class="label"><label{% trans '>Description' %}</label>
						</div>	
		                <div class="value">{{video.description}}</div>	
		            </div>
		           	 <div>{% trans "Video View" %}
					 		<h3>{{video.video_view}}</h3> 
		           	 	</div>
				</div>    
			</div>    
           
    <script type="text/javascript" src="{{STATIC_URL}}ui/js/inline.js"></script> 

</div>


 
 
 
 
 
 
 
 
 
 
 
 
 