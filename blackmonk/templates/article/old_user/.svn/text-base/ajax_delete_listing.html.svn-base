{% load ds_utils %}
{% load i18n %}		
	{% if articles %}
		{% for article in articles %}
		{% for ni in new_id %}
		{% ifequal ni article.id %}		
			<li class="table-list" id="li_{{article.id}}">
			        <div class="table-list-container check-item group">
			        		<span id="id_featured_cls_{{article.id}}" class="list-type {% if article.featured %}featured-item{% endif %}"><a class="tttxt" title="{% if article.featured %}{% trans 'Item listed as featured' %}{% endif %}" href="#">{% if article.featured %}{% trans 'Featured' %}{% endif %}</a></span>
						<span class="checkbox-wrap">
							<span class="cform-checkbox-container"><input type="checkbox" value="{{article.id}}" id="id_event_action" name="artCulture" class="check-overflow cbox"><span class="cform-checkbox-element"></span></span>
						</span>
			            <div class="item-info relative-block">
			                <h3 class="title"><a href="{% url 'user_article_preview' article.id %}">{{article.title}}</a></h3>
			                <p class="ltxt80">{{article.type}} <span class="divider12">|</span> {% trans 'By' %} <a href="{% url 'user_profile' article.created_by.profile_slug %}">{{article.created_by.dispaly_name}}</a> <span class="divider12">|</span>{{article.created_on|date:"M d, Y"}}</p>
			                <ul class="quick-stats group">
			                    <li class="views"><a class="tttxt" href="javscript:void(0)" title="{% trans 'Viewers' %}"><span class="inline-block icon-view-stats"></span>{{article.most_viewed}}</a></li>
			                    <!--li class="cmnt"><a class="tttxt"title="{% trans 'View comments' %}" href="#"><span class="inline-block icon-cmnt-stats"></span></a></li-->
			                </ul>
			                <div class="item-status no-options dev_hide_list_status">
			                	<span class="status-inside inline-block" id="id_span_chge_sts_{{article.id}}">
		                		
                                	<span title="{% get_status_class article.status %}" id="id_estatus_{{article.id}}" name="{{article.status}}" class="inline-block tttxt status-idty icon-{% get_status_class article.status %}"></span>	  
                                
			                    <!--span class="arrow"></span--></span>
								<!--ul class="dropdown-list" id="change_status_id_{{article.id}}">
									<li><strong>{% trans 'Update status to...' %}</strong></li>
			                       	<ul id="id_dis_status_{{article.id}}">
			                       	</ul>
			                    </ul-->
			                </div>                                                        
			            </div>
			            <div class="action-buttons {% if forloop.counter == 1 %}tip tip6{% endif %}">
			                <div class="relative-block">
			                    <span class="item-manage inline-block dev_hide_list_settings" id="id_span_settings_{{article.id}}" onclick="display_settings({{article.id}});"><span class="action-icon icon-manage"></span></span>
			                    <ul class="dropdown-list dev_settingsdrpdwn" id="settings_id_{{article.id}}" style="display:none">
			                        <li><a href="{% url 'article_add_article' %}?aid={{article.id}}">{% trans 'Edit this article' %}</a></li>
									<li><a title="{% trans 'Title / Description (SEO)' %}" href="{% url 'article_update_seo' article.id %}" class="bm-add-gadget light_box">{% trans 'Update SEO' %}</a></li>
			                       	<li><span class="line-break"></span></li>
								    <li><a onclick="if(confirm(gettext('Are you sure you want to delete the selected article?'))){filter_action({{article.id}},'DEL');return false;}" href="javascript:void(0)">{% trans 'Delete this article' %}</a></li>
			                    </ul>
							</div>                                                              	                                                         
			            </div>
			        </div>
			    </li> 
			{% endifequal %}
		 {% endfor %}
		 {% endfor %}
	{% else %}
		  	<div class="no-data"> <p>{% trans 'Sorry , No articles found !' %}</p></div>  
    {% endif %}