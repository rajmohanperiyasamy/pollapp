 {% load i18n %}
 {% if is_paginated %}
      <div class="base-page-nav">
            <ul class="page-nav">
              	{% if has_previous %}
					<li class="val_previous"><a href=javascript:void(0) onclick="goto_list('{{ prev }}');return false;" id="{{ prev }}"><span class="inline-block icon-prev">&lsaquo;</span></a></li>
                {% else %}
					 <li class="val_previous"><a href="javascript:void(0)" id="{{ prev }}" class="inactive"><span class="inline-block icon-prev">&lsaquo;</span></a></li>
                {% endif %}
                
                {% if not in_leading_range %}
					{% for num in pages_outside_trailing_range %}
						{% ifequal num current_page %}
							 <li><span title="{% trans 'Page' %} {{ num }} " class="active">{{ num }}</span></li>
						{% else %}
							 <li><a title="{% trans 'Page' %} {{ num }} " href=javascript:void(0) onclick="goto_list('{{ num }}');return false;">{{ num }}</a></li>
						{% endifequal %}
					{% endfor %}
						...
			    {% endif %} 
                 
                {% for num in page_numbers %}
					{% ifequal num current_page %}
						 <li><span title="{% trans 'Page' %} {{ num }} " class="active">{{ num }}</span></li>
					{% else %}
						<li><a title="{% trans 'Page' %} {{ num }} "href=javascript:void(0) onclick="goto_list('{{ num }}');return false;">{{ num }}</a></li>
					{% endifequal %}
				{% endfor %}
                 
                {% if not in_trailing_range %}
					...
					{% for num in pages_outside_leading_range reversed %}
						{% ifequal num current_page %}
							 <li><span title="{% trans 'Page' %} {{ num }} " class="active">{{ num }}</span></li>
						{% else %}
							<li><a title="{% trans 'Page' %} {{ num }} " href="javascript:void(0)" onclick="goto_list('{{ num }}');return false;">{{ num }}</a></li>
						{% endifequal %}
					{% endfor %}
				{% endif %}
				
				{% if has_next %}
					<li class="val_next"><a href=javascript:void(0) onclick="goto_list('{{ next }}');return false;"" id="{{next}}" ><span class="inline-block icon-next">&rsaquo;</span></a></li>
				{% else %}
					 <li class="val_next"><a href="javascript:void(0)" id="{{next}}" class="inactive"><span class="inline-block icon-next">&rsaquo;</span></a></li>
				{% endif %}
		  </ul>
	</div>   
 {% endif %}