<div id="stats" class="group content">
	<h2>Type - {{ feedback.type }}</h2>
	<div class="col group">
		<p class="group">      
            <span class="property">Posted By </span>
            <span class="value">{{ feedback.name }}</span>
        </p> 
<p class="group">      
            <span class="property">Contact Email Address</span>
            <span class="value">{{ feedback.email }} </span>
        </p>
    </div>
    <div class="col group referral-url ">
    	<h4 class="col-head">Details </h4>
		 
					<p class="group"> 
						<span class="property">Module/Section </span> 
						<span class="value">{{ feedback.module }}</span>
					</p>
                    <p class="group"> 
						<span class="property">Note </span> 
						<span class="value">{{ feedback.message }}</span>
					</p>
    </div>
</div>

<div class="modal-base  group">
       <button role="button" class="cform-button prime right" onclick="$.colorbox.close();" type="button"><span class="cform-button-text">OK</span></button>
    </div>