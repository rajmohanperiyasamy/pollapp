function clear_top_info_bar(){
	$('#id_search').val('');
	$('#id_search_keyword').val('');
	$('#id_search_keyword1').val('');
	$('#search_category').val('');
	$('#id_pagentn').val('1');
	
	$('#id_status').val('all');
	$('#id_listing_type').val('all');
	$('#id_created').val('all');
	
	$('#filter_a a').removeClass('active');
	$('#filter_b a').removeClass('active');
	$('#filter_c a').removeClass('active');
	
