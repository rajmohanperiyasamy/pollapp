<script type="text/javascript">
   	 $(document).ready(function(e) {
		 var length = $('ul.module-nav li').length;
		 var last_child = $("ul.module-nav li:nth-child(13)");
		 if(length > 14){
		var ex_li = last_child.nextAll();
		ex_li.remove;
		$('ul.module-nav').css('overflow','visible');
		last_child.after('<li class="more"> <a href="JavaScript:void(0);"> <span class="icon_cont"> <span class="more_icon"></span> <span class="more_icon"></span> <span class="more_icon"></span> </span> <span class="title">More</span> </a> <div class="module_extra_nav_cont"> <span class="arrow"></span> <ul class="module-nav vertical group"> </ul> </div></li>')
		 $('ul.module-nav.vertical').append(ex_li);
		 var added_length = $('ul.module-nav.vertical li').length;
		 $('.module_extra_nav_cont ul.module-nav.vertical').css('width',70*added_length+5);
		$('.module_extra_nav_cont').css('display','none');
		if($('ul.module-nav.vertical li').hasClass('active')){
			last_li = last_child.html();
			last_li_class = last_child.attr('class');
			active_li = $('ul.module-nav.vertical li.active').html()
			active_li_class = $('ul.module-nav.vertical li.active').attr('class');
			last_child.empty().html(active_li);
			last_child.removeClass(last_li_class);
			last_child.addClass(active_li_class);
			$('ul.module-nav.vertical li.active').empty().html(last_li);
			$('ul.module-nav.vertical li.active').addClass(last_li_class);
			$('ul.module-nav.vertical li.active').removeClass(active_li_class);	
		}
		$('.module-nav.vertical li').click(function(){
		setTimeout(function() {
		$('.module_extra_nav_cont').removeClass('active');
		}, 100);
		setTimeout(function() {
		$('.module_extra_nav_cont').css('display','none');
		}, 350);
		})
		 } 
		$('ul.module-nav li.more').mouseenter(function(){
		$('.module_extra_nav_cont').css('display','block');
		setTimeout(function() {
		$('.module_extra_nav_cont').addClass('active');
		}, 01);
		})
		$('ul.module-nav li.more').mouseleave(function(){
		$('.module_extra_nav_cont').removeClass('active');
		setTimeout(function() {
		$('.module_extra_nav_cont').css('display','none');
		}, 300);
		})
    });
   </script>

<script type="text/javascript">
$(document).ready(function(){
	$('.check_action').removeAttr('checked');
	$('body').css('pointer-events','');
	$('#id_search_keyword').bind('keypress', function(e) {
		var code = (e.keyCode ? e.keyCode : e.which);
	 	if(code == 13) {
			$('#id_search').val('true')
			sort_search();
		}
	});

	$('.todo-button').click(function(event){
				var classopen;
				event.stopPropagation();
				if($(this).parent(".mb-signal").hasClass('open')){classopen=true}
				$('.mb-signal').removeClass('open');
				if(classopen){$(this).parent(".mb-signal").addClass('open')}
				$(this).parent(".mb-signal").toggleClass('open')
			});
			$('#signal-scrollbar .scrollbar,.signal-content-header,.dropdown-arrow .arrow').click(function(event){
				event.stopPropagation();
			});
			$('.account-bar,#header,#content').click(function(){
				$('.mb-signal').removeClass('open')
	});	
	$('.notify-button').click(function(event){
				var classopen;
				event.stopPropagation();
				if($(this).parent(".mb-signal").hasClass('open')){classopen=true}
				$('.mb-signal').removeClass('open');
				if(classopen){$(this).parent(".mb-signal").addClass('open')}
				$(this).parent(".mb-signal").toggleClass('open')
				reset_new_notfication();
			});
			$('#signal-scrollbar .scrollbar,.signal-content-header,.dropdown-arrow .arrow,.scrollbar').click(function(event){
				event.stopPropagation();
			});
			$('.account-bar,#header,#content').click(function(){
				$('.mb-signal').removeClass('open')
	});	
	
	show_todo_list();	
	show_notification();
	$('#master_check').live("click",function(){
		if($("#master_check").is(':checked')){$(".cbox").attr('checked',true);}
		else{$(".cbox").attr('checked',false);}
		fix_action();
	});
	$('.cbox').live("click",function(){
		var flag=true;
		$('.cbox').each(function(){if($(this).is(':checked')==false){flag=false}})
		if(flag){$("#master_check").attr('checked',true);}
		else{$("#master_check").attr('checked',false);}
	});
});
function fix_action(){
	var P=0;
	var R=0;
	var B=0;
	var N=0;
	var E=0;
	var D=0;
	$('.tool.action_status .chzn-container').removeClass('chzn-disabled , disabled_chosen');
	$('.action_status .group-result').removeClass('disabled');
	$(".publish_action").hide();
	$(".reject_action").hide();
	$(".block_action").hide();
	$(".feature_action").hide();
	$('.check_action').each( function(index) {
		if ($(this).is(':checked'))
		{
			//alert($(this).attr('status')); 
			if($(this).attr('status') == 'P'){P=P+1;}
			if($(this).attr('status') == 'S'){P=P+1;}
			if($(this).attr('status') == 'R'){R=R+1;}
			if($(this).attr('status') == 'B'){B=B+1;}
			if($(this).attr('status') == 'N'){N=N+1;}
			if($(this).attr('status') == 'D'){D=D+1;}
			if($(this).attr('status') == 'E'){E=E+1;}
		}
	});
	//alert("P:"+P+" R:"+R+" B:"+B+" N:"+N+" D:"+D);
	if(E == 0 ){
		var Group=0;
		if(P == 0 ){$(".publish_action").show();Group=1;}
		if(R == 0 && B==0 && P==0 && D==0){$(".reject_action").show();Group=1;}
		if(B == 0 && N==0 && D==0){$(".block_action").show();Group=1;}
		if(P > 0 && (R == 0 && B==0 && N==0 && D==0)){$(".feature_action").show();Group=1;}
		if(Group == 0){$('.action_status .group-result').addClass('disabled'); }
	}
	else{$('.action_status .group-result').addClass('disabled');}
	//alert((P == 0 && B==0) + (R == 0 && B==0 && P==0) + (B == 0 && N==0));
	if( P==0 && R==0 && B==0 && N==0 && E==0 && D==0){$('.tool.action_status .chzn-container').addClass('chzn-disabled , disabled_chosen')}
}
// Ajax To Do list Start
function show_todo_list(){
	$('#ajax_to_do_loading').show();
	$.ajax({
		type: "POST",
		url: "{% url 'staff_ajax_todo_list' %}",
		data: 0,
		dataType:'JSON',
		success: function(data){
				if(data.status=='1'){
					$('#ajax_to_do_loading').hide();
					if(data.total_cnt!='0'){
						$('#todos-count-value').html(data.total_cnt);
						$('#ajax_todo_loading').empty().append(data.temp);
						$('#todos-signal').addClass('has-new');
					}
					scroll_bar();
				}
			}
		});
}	
// Ajax To Do list End
// Ajax Notification list Start
function show_notification(){
	$('#ajax_notification_loading').show();
	$.ajax({
		type: "POST",
		url: "{% url 'staff_ajax_notification' %}",
		data: 0,
		dataType:'JSON',
		success: function(data){
				if(data.status=='1'){
					$('#ajax_notification_loading').hide();
					$('#notifications-count-value').html(data.not_cnt);
					$('#ajax_not_loading').empty().append(data.temp);
					if(data.not_cnt!='0'){
						$('#notifications-signal').addClass('has-new');
					}
					scroll_bar();
				}
			}
		});
}
function reset_new_notfication(){
	if($('#notifications-count-value').html()!='0'){
	$.ajax({
		type: "POST",
		url: "{% url 'staff_reset_notification' %}",
		data: 0,
		success: function(data){
				if(data=='1'){
						try{$('#notifications-signal').removeClass('has-new');}
						catch(err){}
					}
			    }
		});
	}	
}
// Ajax Notification list End
//scrolbar start

function scroll_bar(){
	todo_body_height = $('#todos-signal .signal-body').height()
	notification_body_height = $('#notifications-signal .signal-body').height()
	if(todo_body_height >= 319) {
	$('#todos-signal .viewport').css('height','319px');
	}
	else {
	$('#todos-signal .viewport').css('height',todo_body_height);
	}
	if(notification_body_height >= 319) {
	$('#notifications-signal .viewport').css('height','319px');
	}
	else {
	$('#notifications-signal .viewport').css('height',notification_body_height);
	}
	$('.signal-scrollbar').tinyscrollbar();	
	
}
//scrolbar end

