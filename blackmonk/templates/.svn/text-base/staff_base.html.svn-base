}
function hide_top_info_bar(){$('#top_info_bar').hide();}
function get_status_text(status){
	if(status=='P'){return gettext('Published');}
	else if(status=='N'){return gettext('Pending');}
	else if(status=='D'){return gettext('Drafted');}
	else if(status=='B'){return gettext('Blocked');}
	else if(status=='R'){return gettext('Rejected');}
	else if(status=='S'){return gettext('Scheduled');}
	else if(status=='E'){return gettext('Expired');}
	else return '';
}
function get_listingtype_text(listingtype){
	if(listingtype=='F'){return gettext('Featured');}
	else if(listingtype=='S'){return gettext('Sponsored');}
	else if(listingtype=='B'){return gettext('Free');}
	else if(listingtype=='1'){return gettext('Featured');}
	else if(listingtype=='0'){return gettext('Basic');}
	else if(listingtype=='NF'){return gettext('Not Featured');}
	else if(listingtype=='FL'){return gettext('Flaged');}
	else return '';
}
function get_created_text(created){
	if(created=='CSM'){return gettext('Your');}
	else if(created=='CSO'){return gettext('Others');}
	else return '';
}
function show_top_info_bar(module){
	var module=gettext(module);
	var txt_status=get_status_text($('#id_status').val());
	var txt_listingtype=get_listingtype_text($('#id_listing_type').val());
	var txt_by=get_created_text($('#id_created').val());
	
	var a_link=" <a class='text-link'  href='javascript:clear_top_info_bar()'>"+gettext("Clear filter")+"</a>";
	var txt=gettext('Showing ');
	var txt_all='';
	var search=false;
	var search_text='';
	
	if($('#id_search').val()!='' && $('#id_search').val()!=null){
		search=true;
		search_text=$('#id_search_keyword').val();
	}
	
	if(search){
		if(search_text!=''){txt+=gettext('search result for ')+'"'+search_text+'" '+module+a_link;}
		else{txt+=gettext('search result for ')+module+a_link;}
		$('#top_info_bar').empty().html(txt);
		$('#top_info_bar').show();
	}
	else{
		if(txt_status!='' || txt_listingtype!='' || txt_by!=''){
			
			if(txt_status!=''){txt_all=txt_status;}
			if(txt_listingtype!=''){txt_all=txt_listingtype;}
			if(txt_by!=''){txt_all=txt_by;}
			
			if(txt_status!='' && txt_listingtype!='' ){txt_all=txt_status+gettext(' and ')+txt_listingtype;}
			if(txt_status!='' && txt_by!=''){txt_all=txt_status+gettext(' and ')+txt_by;}
			if(txt_listingtype!='' && txt_by!=''){txt_all=txt_listingtype+gettext(' and ')+txt_by;}
			
			if(txt_status!='' && txt_listingtype!='' && txt_by!=''){
				txt_all=txt_status+','+txt_listingtype+gettext(' and ')+txt_by;
			}
			
			txt+='"'+txt_all+'" '+module+a_link;
			$('#top_info_bar').empty().html(txt);
			$('#top_info_bar').slideDown(200,'linear');
		}
		else{$('#top_info_bar').hide();}
	}
}
