<script type="text/javascript">
var mySource = [{ id: 1, name: '12:00 AM'}, { id: 2, name: '12:30 AM'}, { id: 3, name: '01:00 AM'}, { id: 3, name: '01:30 AM'}, { id: 3, name: '02:00 AM'}, { id: 3, name: '02:30 AM'}, { id: 3, name: '03:00 AM'}, { id: 3, name: '03:30 AM'}, { id: 3, name: '04:00 AM'}, { id: 3, name: '04:30 AM'}, { id: 3, name: '05:00 AM'}, { id: 3, name: '05:30 AM'}, { id: 3, name: '06:00 AM'}, { id: 3, name: '06:30 AM'}, { id: 3, name: '07:00 AM'}, { id: 3, name: '07:30 AM'}, { id: 3, name: '08:00 AM'}, { id: 3, name: '08:30 AM'}, { id: 3, name: '09:00 AM'}, { id: 3, name: '09:30 AM'}, { id: 3, name: '10:00 AM'}, { id: 3, name: '10:30 AM'}, { id: 3, name: '11:00 AM'}, { id: 3, name: '11:30 AM'},{ id: 1, name: '12:00 PM'}, { id: 2, name: '12:30 PM'}, { id: 3, name: '01:00 PM'}, { id: 3, name: '01:30 PM'}, { id: 3, name: '02:00 PM'}, { id: 3, name: '02:30 PM'}, { id: 3, name: '03:00 PM'}, { id: 3, name: '03:30 PM'}, { id: 3, name: '04:00 PM'}, { id: 3, name: '04:30 PM'}, { id: 3, name: '05:00 PM'}, { id: 3, name: '05:30 PM'}, { id: 3, name: '06:00 PM'}, { id: 3, name: '06:30 PM'}, { id: 3, name: '07:00 PM'}, { id: 3, name: '07:30 PM'}, { id: 3, name: '08:00 PM'}, { id: 3, name: '08:30 PM'}, { id: 3, name: '09:00 PM'}, { id: 3, name: '09:30 PM'}, { id: 3, name: '10:00 PM'}, { id: 3, name: '10:30 PM'}, { id: 3, name: '11:00 PM'}, { id: 3, name: '11:30 PM'}];
$(document).ready(function(){
	$('#id_mon_start').typeahead({ source: mySource });
	$('#id_mon_end').typeahead({ source: mySource });
	$('#id_tue_start').typeahead({ source: mySource });
	$('#id_tue_end').typeahead({ source: mySource });
	$('#id_wed_start').typeahead({ source: mySource });
	$('#id_wed_end').typeahead({ source: mySource });
	$('#id_thu_start').typeahead({ source: mySource });
	$('#id_thu_end').typeahead({ source: mySource });
	$('#id_fri_start').typeahead({ source: mySource });
	$('#id_fri_end').typeahead({ source: mySource });
	$('#id_sat_start').typeahead({ source: mySource });
	$('#id_sat_end').typeahead({ source: mySource });
	$('#id_sun_start').typeahead({ source: mySource });
	$('#id_sun_end').typeahead({ source: mySource });
});
$('.workinghour').bind("change", function(){
    $("#mform").validate().element($(this));
});

//jQuery(function(){time=$('.W80').autocomplete({source:availableTags});});
	$('#apply_all').click(function(){
	    $('.starttime.on').val($('#id_mon_start').val());
		$('.endtime.on').val($('#id_mon_end').val());

		if($('#id_closed_1').prop('checked')){
			$('.w_closed').attr('checked','checked');
			$('.starttime').attr('disabled','disabled')
			$('.endtime').attr('disabled','disabled')
		}
		if($('#id_allday_1').prop('checked')){
			$('.w_open').attr('checked','checked');
		}
		if(!$('#id_closed_1').prop('checked')){
			$('.w_closed').removeAttr('checked');
			$('.starttime').removeAttr('disabled')
			$('.endtime').removeAttr('disabled')
			$('.starttime').addClass('on')
			$('.endtime').addClass('on')
			if($('#id_mon_start').val() == ''){
				$('.starttime').attr('placeholder','09:00 AM');
			}
			if($('#id_mon_end').val() == ''){
				$('.endtime').attr('placeholder','06:00 PM');
			}
		}
		if(!$('#id_allday_1').prop('checked')){
			$('.w_open').removeAttr('checked');
		}
	});
	$('.w_closed').click(function(){
		if($(this).is(':checked')){
			$(this).parent().parent().find('input[type=text]').val('');
			$(this).parent().parent().find('input[type=text]').attr('placeholder','');
			$(this).parent().parent().find('input[type=text]').attr('disabled','disabled');
			$(this).parent().parent().find('input[type=text]').removeClass('on');
			$(this).parent().parent().find('.w_open').removeAttr('checked');
		}
		else{
			$(this).parent().parent().find('.starttime').attr('placeholder','09:00 AM');
			$(this).parent().parent().find('.endtime').attr('placeholder','06:00 PM');
			$(this).parent().parent().find('input[type=text]').removeAttr('disabled');
			$(this).parent().parent().find('input[type=text]').addClass('on');
		}
	});
	$('.w_open').click(function(){
		if($(this).is(':checked')){
			$(this).parent().parent().find('.starttime').val('12:00 AM');
			$(this).parent().parent().find('.endtime').val('12:00 AM');
			$(this).parent().parent().find('input[type=text]').removeAttr('disabled');
			$(this).parent().parent().find('input[type=text]').addClass('on');
			$(this).parent().parent().find('.w_closed').removeAttr('checked');
		}
	});
</script>