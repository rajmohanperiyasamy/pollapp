	}
	
	try{logo_upload('{% url 'business_ajax_upload_logo' %}?id={{business.id}}',1);}
	catch(e){}
	$('#fileupload').bind('fileuploadstart', function (e, data) {$('#upload-button-wrapper').hide();});
