{% load thumbnail %}
<script type="text/javascript">
var main_url = "{{ globalsettings.website_url }}";
</script>
<!-- script type="text/javascript" src="{{STATIC_URL}}ui/js/application.js"></script -->
<input type="hidden" value="{{status}}" name="id_status" id="id_status">
<input type="hidden" value="{{listing_type}}" name="id_listing_type" id="id_listing_type">
<input type="hidden" value="{{created}}" name="id_created" id="id_created">
<input type="hidden" value="{{sort}}" name="id_sort" id="id_sort">
<input type="hidden" value="" name="id_action" id="id_action">
<input type="hidden" value="{% url 'user_banners_ajax_banner_actions' %}" name="id_banner_action" id="id_banner_action_url">
<input type="hidden" value="{% url 'user_banners_ajax_list_banners' %}" name="id_banner_listing_url" id="id_banner_listing_url">
<input type="hidden" value="{% url 'user_banners_ajax_banner_state' %}" name="banner_state_url" id="id_banner_state_url">
<input type="hidden" value="{% if search %}{{search}}{% endif %}" name="id_search" id="id_search">
{% if banners %}
<ul class="modulesBrowseData dSrS480 mDa-lT mDa-oT-lT oVrfLw-vSbL">
	{% for buz in banners %}
    <li class="mDa mDa-sTtS-iN">
        <div class="mDa-iN">
            <div class="mDa-wPr"> 
            	<a class="bUi-tMb-wPr tMb-75 dSpD  fLxiMgW bUi-tMb-110" href="{% url 'user_banners_add_banner' %}?bid={{buz.id}}">
                    <span class="bUi-tMb w100pXmP">
                        <span class="bUi-tMb-pRpL">
                            <span class="bUi-tMb-cLp">
                                <span class="bUi-tMb-cLp-iN"> <img class="mDa-oT" src="{% if buz.image %}{% thumbnail buz.image 110x0 %}{% else %}{{buz.get_default_image}}{%endif%}" alt="">
                                    <span class="vRtL-aLn"> </span>
                                </span>
                            </span>
                        </span>
                    </span>
                </a>
            </div>
            <div class="mDa-bDy mRr0mP">
                <h5 class="mDa-hD tRnCt0 mR50 rLvE h65" title="{{buz.caption}}"><a href="{% url 'user_banners_add_banner' %}?bid={{buz.id}}">{{buz.caption}}</a></h5>
                <p class="fS12 cLr-gRy"><span class="fS11 cLr-lTgRy">{{buz.created_on|date:"F d, Y f A"}}</span></p>
                <div class="clearfix">
                	<div class="btn-group pLlT iM" id="id_drop_{{buz.id}}">
                        <a href="{% url 'user_banners_add_banner' %}?bid={{buz.id}}" title="Edit" class="btn btn-light btn-small"><span id="id_delete_{{buz.id}}"><i aria-hidden="true" class="bUi-iCn-rVeW-12 ib"></i></span><span id="id_loding_{{buz.id}}" style="display:none;"><i alt="Loading icon" class="icon-spinner bUi-iCn-sPnR mRr5 fS16" aria-hidden="true"></i></span></a>
                        <button class="h27 btn btn-light btn-small dropdown-toggle" data-toggle="dropdown">
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu mNi pLrT dSpL">
							<!--li><a title="Traffic Reports" href="{% url 'user_banners_banner_traffic_reports' %}?bid={{buz.id}}">Traffic reports</a></li-->
                            <li><a onclick="if(confirm('Are you sure you want to delete the selected banner?')){filter_action({{buz.id}},'DEL');return false;}" href="javascript:void(0)">Delete</a></li>
                        </ul>
                    </div>
                    <div class="pLrT mRr10 iM" style="cursor:pointer;">
                        <i {% if buz.status == 'P' %}class="bUi-iCn-pPiE-24 cLr-pUb" title="Published"{% endif %}
							{% if buz.status == 'R' %}class="bUi-iCn-rGt-24 cLr-rEj" title="Rejected"{% endif %}
							{% if buz.status == 'N' %}class="bUi-iCn-pNdG-24 cLr-pEn" title="Pending"{% endif %}
							{% if buz.status == 'D' %}class="bUi-iCn-dRaT-24 cLr-dRf" title="Drafted"{% endif %}
							{% if buz.status == 'B' %}class="bUi-iCn-bLk-24 cLr-bLoK" title="Blocked"{% endif %}
							{% if buz.status == 'E' %}class="bUi-iCn-bLk-24 cLr-bLoK" title="Expired"{% endif %} aria-hidden="true"></i>
                    </div>	
                </div>
                <div class="mDa-lT-sTtS aBsLt tR0 StCmP tXt-rTp bGtRp">
