{% load ds_utils %}
{% load i18n %}	
{% load thumbnail %}		

{% if product_objs %}
	{% for product_obj in product_objs %}
	{% for ni in new_id %}
	{% ifequal ni product_obj.id %}			
	<li class="table-list" id="li_{{product_obj.id}}">
	        <div class="table-list-container check-item has-thumb110 group">
        		<span id="id_featured_cls_{{product_obj.id}}" class="list-type {% if product_obj.featured %}featured-item{% endif %}"><a class="tttxt" title="{% if product_obj.featured %}{% trans 'Item listed as featured' %}{% endif %}" href="#">{% if product_obj.featured %}{% trans 'Featured' %}{% endif %}</a></span>
				<span class="checkbox-wrap">
					<span class="cform-checkbox-container">
						<input type="checkbox" onclick="fix_action()" status="{{product_obj.status}}" value="{{product_obj.id}}" id="id_event_action" name="artCulture" class="check-overflow cbox check_action">
					<span class="cform-checkbox-element"></span></span>
				</span>
				<div class="item-media">
                	<a href="{% url 'staff_bmshop_preview_product' product_obj.id %}" class="inline-block">
                        <span class="item-thumb thumb-110">
                            <span class="thumb-in">
                                <span class="thumb"><img src="{% if product_obj.get_cover_image_staff %}{% thumbnail product_obj.get_cover_image_staff 110x0 %}{% else %}{{product_obj.get_default_image_staff}}{%endif%}" alt="{{product_obj.name}}" class="inline-block"><span class="inline-block h100"></span></span>
                            </span>
                        </span>
					</a>                                                            
                </div> 	
	            <div class="item-info relative-block">
	                <h3 class="title"><a href="{% url 'staff_bmshop_preview_product' product_obj.id %}">{{product_obj.name}}</a></h3>
	                <p class="ltxt80">{% for cat in product_obj.categories.all %}{{cat.name}} {%if not forloop.last%},{% endif %}{% endfor %} <span class="divider12">|</span> {% trans 'By' %} <a href="{% url 'user_profile' product_obj.created_by.profile_slug %}">{{product_obj.created_by.display_name}}</a> <span class="divider12">|</span>{{product_obj.created_on|date:"M d, Y"}}<span class="divider12">|</span>Id: {{product_obj.uid}}</p>
	                <ul class="quick-stats group">
	                    <li class="views"><a class="tttxt" href="javascript:void(0)" title="{% trans 'Viewers' %}"><span class="inline-block icon-view-stats"></span>{{product_obj.most_viewed}}</a><a class="tttxt" href="javascript:void(0)" title="{% trans 'Product Id' %}">{{product_obj.uid}}</a></li>
	                    <!--li class="cmnt"><a class="tttxt"title="{% trans 'View comments' %}" href="#"><span class="inline-block icon-cmnt-stats"></span></a></li-->
	                </ul>
	                <div class="item-status dev_hide_list_status" onclick="display_change_status({{product_obj.id}},$('#id_estatus_{{product_obj.id}}').attr('name'));">
	                	<span class="status-inside inline-block" id="id_span_chge_sts_{{product_obj.id}}">
                        <span title="{% get_status_class product_obj.status %}" id="id_estatus_{{product_obj.id}}" name="{{product_obj.status}}" class="inline-block tttxt status-idty icon-{% get_status_class product_obj.status %}"></span>	  
							<span class="arrow"></span></span>
							<ul class="dropdown-list" id="change_status_id_{{product_obj.id}}">
								<li><strong>{% trans 'Update status to...' %}</strong></li>
		                       	<ul id="id_dis_status_{{product_obj.id}}">
		                       	</ul>
		                    </ul>
	                </div>                                                        
	            </div>
	            <div class="action-buttons">
	                <div class="relative-block">
	                    <span class="item-manage inline-block dev_hide_list_settings" id="id_span_settings_{{product_obj.id}}" onclick="display_settings({{product_obj.id}});"><span class="action-icon icon-manage"></span></span>
	                    <ul class="dropdown-list dev_settingsdrpdwn" id="settings_id_{{product_obj.id}}" style="display:none">
								<li id="id_feature_{{product_obj.id}}" {% if product_obj.featured%}style="display:none"{%endif%}><a href="javascript:void(0)" onclick="feature_product({{product_obj.id}},'F')">{% trans 'Feature this' %}</a></li>
								<li id="id_unfeature_{{product_obj.id}}" {% if not product_obj.featured%}style="display:none"{%endif%}><a href="javascript:void(0)" onclick="feature_product({{product_obj.id}},'N');">{% trans 'Non-feature this' %}</a></li>
								<li><span class="line-break"></span></li>
								<li><a href="{% url 'staff_bmshop_edit_product' product_obj.id %}">{% trans 'Edit Product' %}</a></li>
								<li><a href="{% url 'staff_bmshop_product_property' product_obj.id %}">{% trans 'Edit Property' %}</a></li>
								<li><a title="{% trans 'Title / Description (SEO)' %}" href="{% url 'staff_bmshop_product_seo' product_obj.id  %}" class="bm-add-gadget light_box">{% trans 'Update SEO' %}</a></li>
								<li><span class="line-break"></span></li>
								<li><a href="{% url 'staff_bmshop_clone_product' product_obj.id %}">{% trans 'Clone Product' %}</a></li>			                       	
						    	<li><a onclick="if(confirm(gettext('Are you sure you want to delete the selected product(s)?'))){filter_action({{product_obj.id}},'DEL');return false;}" href="javascript:void(0)">{% trans 'Delete' %}</a></li>
	                    </ul>
					</div>                                                              	                                                         
	            </div>
	        </div>
	    </li> 
		{% endifequal %}
	  {% endfor %}
	{% endfor %}
{% endif %}		