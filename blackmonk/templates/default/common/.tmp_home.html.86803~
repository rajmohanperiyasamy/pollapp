        var pathname = window.location.pathname;
        org_path=pathname;
        $(document).ready(function(){
        var bids=new Array();
        var aids=new Array();
        var eids=new Array();
        var nids=new Array();
        var dids=new Array();
        var gids=new Array();
        var vids=new Array();
        var module=['/business/','/events/','/article/','/news/','/videos/','/gallery/','/deal/']
        var category=new Array();
        var category_name='';
        
         $('.bids').each(function(){
                bids.push($(this).val());
            });
        
         $('.eids').each(function(){
                eids.push($(this).val());
            });
        
         $('.aids').each(function(){
                aids.push($(this).val());
            });
        
         $('.vids').each(function(){
                vids.push($(this).val());
            });
         $('.nids').each(function(){
                nids.push($(this).val());
            });
        
         $('.dids').each(function(){
                dids.push($(this).val());
            });
         $('.gids').each(function(){
                gids.push($(this).val());
         });
        reports_data(module[0],bids,category,org_path)
        reports_data(module[1],eids,category,org_path)
        reports_data(module[2],aids,category,org_path)
        reports_data(module[3],nids,category,org_path)
        reports_data(module[4],vids,category,org_path)
        reports_data(module[5],gids,category,org_path)
        reports_data(module[6],dids,category,org_path)
        });
