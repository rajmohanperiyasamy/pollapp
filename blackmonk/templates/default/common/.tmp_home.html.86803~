<script type="text/javascript">
$(function () {
    {% with featured_deals.0 as deal %}
        {% if deal %}
            $('.defaultCountdown{{deal.id}}').countdown({
                until: +{{ deal.get_remaining_scnd }},
                format: 'HMS',
                layout: '<span class="fS18 fW200 ib mRt4 cLr-dRk" title="Time Left" id="id_time_left_{{deal.id}}"><i aria-hidden="true" class="bUi-iCn-tMe-aLt-16 fS18 mRr5 cLr-dRk hmm"></i>{hnn}:{mnn}:{snn}</span>'
            });
        {% endif %}
    {% endwith %}
    $('#id_find').typeahead({
        ajax: { url: '{% url 'auto_suggest_business' %}',type: "GET",
                triggerLength: 1 },
		itemSelected:function(){
				$('#sform').submit();
		}
    });
    $('#id_location').typeahead({
        ajax: { url: '{% url 'auto_suggest_business_address' %}',type: "GET",
                triggerLength: 1 }
    }); 
    
    
    try{
        $('.ftrdBsnsRty').raty({
            readOnly: true, 
            score: function() {return $(this).attr('data-rating');},
            size: 16
        });
    }
    catch(e){}
    
});

{% if poll %}
	$(document).ready(function(){
		$.ajax({
		    type:'get',
			dataType:'json',
		    url:'{% url 'ajax_load_poll_data' %}',
		    success:function(data){
				if(data.status){
					$('#id_poll_div').html(data.html);
				}else{
					$('#id_poll_div').remove();
				}
			},
		    error:function(data){
			}
		});
	});
{% endif %}

</script>

{% endblock %}                
