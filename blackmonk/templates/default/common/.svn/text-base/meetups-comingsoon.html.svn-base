{% extends base %}
{% load i18n %}
{% load tabs %}
{% block pagetitle %}{{globalsettings.city}} {% trans ' Meetups' %} | {{globalsettings.domain}}{% endblock %}
{% block meta %}
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta name="description" content="{{globalsettings.city}} {% trans 'Meetups' %}" />
	<meta name="keywords" content="{{globalsettings.city}} {% trans 'Meetups' %}" />
{% endblock %}
{% block head %}

	<style>
	div.coming-soon {
    -moz-transform: rotate(-45deg);
	-webkit-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg);
    color: rgba(254, 139, 62, 0.7);
	*color:#e2915b;
    font-size: 90px;
    font-weight: bold;
    left: 185px;
    position: absolute;
    top: 198px;
    z-index: 9999;
	}
	    	.place_list {
	    padding: 0;
	}
	.place_list .item {
	    background-color: #FCFCFC;
	    border-bottom: 1px solid #E6E6E6;
	    padding: 10px;
	    position: relative;
	}
	.place_list .item:hover {
	    background-color: #FFFFCC;
	}
	.place_list .alt {
	    background-color: #FFFFFF;
	}
	.place_list .item .pic {
		float: left;
		width:50px; 
		padding:4px; 
		border:1px solid #e6e6e6; 
		border-radius:5px; 
		background-color:#FFF;
		margin-right:10px;
	}
	.place_list .item .pic a{display:block;}
	.list_info {
	    border-bottom: 2px solid #EEEEEE;
	    margin: 0;
	    padding: 20px 10px 8px;
	}
	.list_info .dttoday {
	    color: #999999;
	    font-size: 12px;
	    font-weight: bold;
	    text-align: right;
	}
	.list_actions {
	    border-bottom: 2px solid #EEEEEE;
	    font-size: 11px;
	    margin: 0;
	    padding: 20px 10px 8px;
	}
	.list_actions label {
	    color: #3D3D3D;
	    font-weight: bold;
	}
	.list_actions select {
	    color: #666666;
	    font-size: 11px;
	    margin-left: 4px;
	    width: 96px;
	}
	.list_actions .sort {
	    width: 272px;
	}
	.list_actions .filter {
	    text-align: right;
	}
	.event_group {
	    border-left: 1px solid #e2e2e2;
	    float: right;
	    font-size: 10px;
	    line-height: 14px;
	    padding-left: 10px;
	    width: 180px;
	}
	.event_group .meta{color:#666; margin-bottom:2px;}
	.event_group .group_name{font-size:12px; color:#C00}
	.event_detail{margin-right: 190px; line-height: 1.25em; margin-left:70px;}
	.event_detail .title{font-size:14px; font-weight:bold; line-height:16px; margin:3px 0 0px;}
	.event_detail .title a{color:#000}
	.event_detail .venue{font-size:11px;color:#06C; margin-top:3px;}
	.event_detail .D_glue{color: #666666;font-size:11px;}
	.event_detail .rsvp{font-size: 10px;color:#090; margin-top:2px;}
	.event_detail .desc{font-size:11px; line-height:14px; color:#333; padding:3px 20px 0px 0;}
	#more_meetups{padding: 10px 10px 10px;}
	#more_meetups a {display: block;text-align: center;width:300px; margin:0 auto}
	.meetup_topics {padding:10px 0px 20px; background-color:#F6F6F6; border-radius:6px;border:2px solid #e6e6e6}
	.meetup_topics li{border-bottom:1px solid #EEE;}
	.meetup_topics li a{display:block;font-size:12px; font-weight:bold; font-family:Arial, Helvetica, sans-serif; color:#333; text-decoration:none ;padding:2px 10px;}
	.meetup_topics li.active, .meetup_topics li:hover {color:#FFF;}
	.meetup_topics li.active a, .meetup_topics li:hover a{color:#C00; text-decoration:underline; background:url(images/ico/arrow_back.png) no-repeat 10px 50% #FEFBF3; padding-left:24px;}
    </style>

{% endblock %}
{% block navigation %}
	{% activetab "topnav" "meetups" %}
	{{ block.super }}
{% endblock %}
