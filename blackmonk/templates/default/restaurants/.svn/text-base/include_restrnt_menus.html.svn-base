<script type="text/javascript">
function retrieve_menus(hobj,mid){
    $('#id_menu_categories li').removeClass('active');
    $(hobj).parent('li').addClass('active');
    $.ajax({
        type: "get",
        url: '{% url 'restaurants_retrieve_menus_by_type' %}?rid={{restaurant.id}}',
        data: 'mid='+mid,
        dataType:'json',
        success: function(data){
                if(data.status){
                    $('#id_menu_list_section').fadeOut(300, function(){
                        $('#id_menu_list_section').empty().html(data.html).fadeIn(300);
                    });
                }
                else{alert('Oops !!! Not able to process your request.');}                
        }
    });
}
</script>