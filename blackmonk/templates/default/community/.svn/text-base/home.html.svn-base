<script type="text/javascript" src="{{ STATIC_URL }}js/ckeditor/ckeditor.js"></script>
<script type="text/javascript">
var inst;
var eeid;
function activate_ckeditor(eid){
	if(inst){
		inst.destroy();
		cancel_comment($('#id_home_text_'+eeid));	
	}
	eeid = eid;
	function updateTextArea(edt){
		CKEDITOR.tools.setTimeout( function(){ 
	        $('#id_home_text_'+eid).val(edt.getData());
	        $('#id_home_text_'+eid).trigger('keyup');
	    }, 0);  
	}
	CKEDITOR.on( 'instanceCreated', function( e ){
		   e.editor.addCss('body { font-size:12px; font-family:Arial, "Times New Roman", Verdana;}');
   	});
	inst = CKEDITOR.replace( 'id_home_text_'+eid ,{					
		customConfig : '/static/js/ckeditor/custome_config.js',
		toolbar : 'Basic_Image',
		height:120,
		filebrowserImageUploadUrl : '/user/articles/uploadimageyui/'
	});
	inst.on('key', function(event) {updateTextArea(inst);});
}
function deactivate_ckeditor(hobj,eid){
	inst.destroy();
	cancel_comment(hobj);
}

</script>
