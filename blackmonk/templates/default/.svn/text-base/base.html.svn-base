    <script type="text/javascript">
		var user_login = false;
        {% if user.is_authenticated %}
            var user_login = true;
        {% endif %}
        $.fn.emulateTransitionEnd = function(duration) {
            return true;
        };
		var comment_reply_id;
		function do_action_or_login(type){
				if(type == 'addanswer'){
					save_answer();}
				else if(type == 'event'){
					event_rsvp();}
			    else if(type == 'postquestion'){
					post_question();}
			    else if(type == 'addpost'){
			        check_add_post();}
			    else if(type == 'subtopic'){
					topic_follow(1);}
			    else if(type == 'unsubtopic'){
				 	topic_follow(2);}
			    else if(type == 'flaganswer'){
					answer_flag();}
				else if(type == 'cmmt'){
					save_comment();}
				else if(type == 'cmmt_com'){
					save_answer_comment();}

				else if(type == 'cmmt_reply'){
					reply_thread(comment_reply_id);
					$('#replay_'+comment_reply_id).click()
				}
				else if(type == 'comm_cmmt_reply'){
					reply_thread(comment_reply_id);
					hobj = $('#replay_'+comment_reply_id);
					load_recomment_form(hobj);
				}
				else if(type == 'review'){
                    $('#write_review_button_signin').hide();
                    $('#write_review_button').show();
                    load_review();
				}
				else if(type == 'followques'){
			   		follow_homeques(1);}
				else if(type == 'unfollowques'){
			   		follow_homeques(2);}
			    else{
			   		if (type == 'follow'){
			   			follow_entry(1);
			   		}else{
			   			follow_entry(2);
			   		}
			   	}
		}

		function login_box(type, comment_rep_id){
            comment_rep_id = (typeof comment_rep_id === "undefined") ? "" : comment_rep_id;
			target = "{% url 'ajax_signin' %}?"+ 'type='+type+ '&val='+comment_rep_id+ '&next=' + $(location).attr('href');
				if(user_login){
                	action = type;
					comment_reply_id = comment_rep_id;
					do_action_or_login(action);
				}else{
				   // load the url and show modal on success
	               $("#lightbox_login .modal-body").load(target, function() {
	                    $("#lightbox_login").modal("show");
	               });
				}
		}

        function lightbox_load_signup_form(type){
            $('#lightbox_login .modal-body').html('<div class="row-fluid mRt20"><div style="" class="tBl-bLk pDt0"><div class="tBl-cEl tXt-cTr"><img align="..." src="/static/ui/images/global/loading.gif"></div></div></div>');
            $("#lightbox_login .modal-body").load('{% url 'ajax_signup' %}?type='+type, function() {});
        }
        function lightbox_load_signin_form(type){
            $('#lightbox_login .modal-body').html('<div class="row-fluid mRt20"><div style="" class="tBl-bLk pDt0"><div class="tBl-cEl tXt-cTr"><img align="..." src="/static/ui/images/global/loading.gif"></div></div></div>');
            $("#lightbox_login .modal-body").load('{% url 'ajax_signin' %}?type='+type, function() {});
        }
    </script>
