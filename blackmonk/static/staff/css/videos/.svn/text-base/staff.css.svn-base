	padding:10px;
	display:block;
	color:#999; 
	border-bottom:1px solid #EEE;	
}
ul.right li ul.dropdown-list li a{
    background: none repeat scroll 0 0 transparent;
    box-shadow: none;
    color: #222222;
    font-size: 11px;
    font-weight: normal;
    padding: 5px 10px;
	border:none;
	display:block;
}
ul.right li ul.dropdown-list li a:hover{
    background: none repeat scroll 0 0 #454545;
    border-radius: 0 0 0 0;
	border:none;
    color: #FFF;
}

