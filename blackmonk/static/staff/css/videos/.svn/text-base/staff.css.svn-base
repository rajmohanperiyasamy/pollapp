.data-table{margin:0px 0px 40px;}
.data-table .table-list{overflow:visible;clear:both;background:none repeat scroll 0 0 rgba(255,255,255,0.5); box-shadow:0px 1px 0px rgba(255,255,255,1) inset,  0px -1px 0px rgba(0,0,0,0.09), 0px 1px 0px rgba(0,0,0,0.09)}
.data-table .table-list:nth-child(2n+2){background:none repeat scroll 0 0 transparent;}
.data-table .table-list .table-list-container{ position:relative;padding:10px 15px;}
.data-table .table-list .table-list-container.check-item{padding-left:54px;}


.data-table .table-list .checkbox-wrap{display: block;left: 15px;margin-top: -10px;position: absolute;text-align: center;top: 50%;width: 31px;}
.data-table .table-list .checkbox-wrap .check-overflow{}

.data-table .table-list .item-info{float: left;width: 355px;}

.data-table .table-list h2{font-size:16px; font-weight:bold; line-height:20px;color:#222;white-space: nowrap;word-wrap: normal;}
.data-table .table-list h3.title{font-size:14px; font-weight:normal; line-height:16px;color:#333;white-space: nowrap;word-wrap: normal;-o-text-overflow: ellipsis;-ms-text-overflow: ellipsis;text-overflow: ellipsis;display:inline-block; max-width:98%; overflow:hidden}
.data-table .table-list p {font-size:11px; margin:0px;line-height:13px;}

.data-table .table-list .action-buttons{float: right;width:140px;}
.data-table .table-list .action-buttons a{display:block;float:right;padding:5px;margin:2px 0 2px 3px;border-radius:3px;/* background:none repeat scroll 0 0 #EEE; box-shadow:0 1px 0 rgba(0, 0, 0, 0.08), 0 18px 2px rgba(255, 255, 255, 0.267) inset, 0 0 2px rgba(0, 0, 0, 0.114) inset, 0 1px 0 #FFFFFF inset; border-color:#D2D2D2 #C8C8C8 #B1B1B1; border-style:solid; border-width:1px;*/}
.data-table .table-list .action-buttons .icon{display: block;height: 16px;position: static;width: 16px;opacity:0.25;margin:0px;}
.data-table .table-list .action-buttons .icon-stats{background:url(../images/icons/b/24.png) no-repeat 50% 50% transparent;}
.data-table .table-list .action-buttons .icon-edit{background:url(../images/icons/b/39.png) no-repeat 50% 50% transparent;}
.data-table .table-list .action-buttons .icon-delete{background:url(../images/icons/b/51.png) no-repeat 50% 50% transparent;}
.data-table .table-list .table-list-container:hover .action-buttons a:hover .icon{opacity:1;}

/*.data-table .table-list .table-list-container:hover {background-color:#FFF}*/
.data-table .table-list .table-list-container:hover .checkbox-wrap {background:#FFF;box-shadow:none}
.data-table .table-list .table-list-container:hover .action-buttons .icon{opacity:0.65} 



.toolbars {padding:10px 15px; border-bottom:1px solid #EEE; background-color:rgba(0,0,0,0.035)}
.toolbars .left{float:left}
.toolbars .right{float:right}
.toolbar > li{display: block;float: left;height: 28px;margin: 0 8px 0 0;overflow: hidden;width: auto;z-index: 99;color:#AAA;line-height:28px;font-size:11px;font-weight:normal;}
.toolbar li .bm-selectmenu{height:25px;float:left;box-shadow:0 1px 0 rgba(0, 0, 0, 0.08), 0 18px 2px rgba(255, 255, 255, 0.267) inset, 0 0 2px rgba(0, 0, 0, 0.114) inset, 0 1px 0 #FFFFFF inset; border-color:#D2D2D2 #C8C8C8 #B1B1B1;}
.toolbar li label{float:left; margin-right:5px;}
.toolbar li .bm-selectmenu-menu li a, .toolbar li .bm-selectmenu-status{padding: 4px 10px 5px;}
.toolbar .mas-toggle a{height:20px;width:24px;display:block;float:left;text-indent:-9999em;}
.toolbar .mas-toggle a.collapse-view{background:url(../images/global/collapse-view.png) no-repeat 50% transparent;}
.toolbar .mas-toggle a.expand-view{background:url(../images/global/expand-view.png) no-repeat 50% transparent;}
.toolbar .mas-toggle a.collapse-view-selected, 
.toolbar .mas-toggle a.expand-view-selected{background: -moz-linear-gradient(center top , #898989 0%, #AAA 100%) repeat scroll 0 0 transparent;}
.toolbar .master-check{width:31px; text-align:center;}
.toolbar .mas-delete {margin-right:15px;}
.toolbar .mas-delete .icon-delete{background:url(../images/icons/b/51.png) no-repeat 50% 50% transparent;display:block;width:16px; height:16px; margin:3px 5px}
.toolbar .mas-delete .inactive{cursor:default}
.toolbar .mas-delete .inactive .icon-delete{opacity:0.15}


