.dropdown-tab-hover{	
   	background: none repeat-x scroll 0 50% #FFF;
	border-radius: 5px 5px 0px 0px;
	color:brown !important;
	box-shadow: 2px 0px 0px rgba(0, 0, 0, 0.1);	
	border-width:1px 1px 0px !important;
	border-style:solid;
	border-color:#A8A8A8 #AAA #A1A1A1 #AAA !important;	
}
ul.right li ul.dropdown-list {
    background: none repeat scroll 0 0 #FFF;
    border-radius: 0 5px 5px 5px;
    box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.2);
    display: none;
    float: left;
    padding: 5px 0;
	margin-top:-1px;
    position: absolute;
    width: 120px;
    z-index: 3;
	border-width:1px 1px 1px;
	border-style:solid;
	border-color:#AAA #AAA #9A9A9A #AAA;
	top:38px;
}
ul.right li ul.dropdown-list li {
    clear: left;
    margin: 0;
    width: 100%;
	line-height:1;
}
