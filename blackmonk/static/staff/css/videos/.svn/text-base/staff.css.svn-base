.cform-list label{cursor:pointer; font-size:13px;}
.cform-list {padding:10px 15px 10px;}
.cform-input-section{padding:0 0 5px 0;}
.cform-list .sub-section{padding:10px 0 5px 20px; display:block}
.cform-radio-element {background: url("http://s.ytimg.com/yt/imgbin/www-refresh-vflcB5eQ_.png") no-repeat scroll -158px -176px transparent;border-radius: 30px 30px 30px 30px;height: 16px;width: 16px;}
.cform-radio-container input:checked + .cform-radio-element {background: url("http://s.ytimg.com/yt/imgbin/www-refresh-vflcB5eQ_.png") no-repeat scroll -111px -24px transparent;}
.cform-checkbox-element {background: url("http://s.ytimg.com/yt/imgbin/www-refresh-vflcB5eQ_.png") no-repeat scroll -81px -24px transparent;border-radius: 3px 3px 3px 3px;height: 16px;width: 16px;}
.cform-checkbox-container input:checked + .cform-checkbox-element {background: url("http://s.ytimg.com/yt/imgbin/www-refresh-vflcB5eQ_.png") no-repeat scroll 0 -200px transparent;}
.cform-radio-container, .cform-checkbox-container {display: inline-block;padding: 1px;position: relative;vertical-align: middle;}
.cform-radio-element, .cform-checkbox-element {border: 1px solid transparent;cursor: pointer;display: inline-block;}
.cform-radio-container input:focus + .cform-radio-element, .cform-checkbox-container input:focus + .cform-checkbox-element {border: 1px solid #4496E7;box-shadow: 0 0 5px #4496E7, 0 0 10px #FFFFFF inset;margin: 0;}
.cform-radio-container input, .cform-checkbox-container input {border: 0 none;cursor: pointer;opacity: 0;outline: 0 none;position: absolute;top: 0;}
input.cform-checkbox {left: 3px;margin: 0;top: 3px;}







.filter-view-tabs ul li label{color:#808080}
label.sort_orderselect{ display:inline;}
ul.right li{ margin-right:0;}
ul.right li span.page-numbers{margin-right:5px; font-size:12px;color:#666}
ul.right li span.page-numbers a.dropdown-tab{padding:6px; 8px;margin-right:4px;color:#666; border:1px solid transparent; position:relative;}
ul.right li span.page-numbers .arrow{border-color: #999 transparent transparent;border-style: solid;border-width: 4px;height: 0;position: relative;right: -2px;top: 11px;width: 0;}
ul.right li a.page-next{-moz-transition: all 0.218s ease 0s;background: -moz-linear-gradient(center top , #FFFFFF, #E9E9E9) repeat scroll 0 0 #f9f9f9;box-shadow:0 1px 0 rgba(0, 0, 0, 0.08), 0 18px 2px rgba(255, 255, 255, 0.267) inset, 0 0 2px rgba(0, 0, 0, 0.114) inset, 0 1px 0 #FFFFFF inset;border-width: 1px;border-style: solid;border-color:#D2D2D2 #C8C8C8 #B1B1B1 #F0F0F0;border-radius: 0px 3px 3px 0px;color: #666666;cursor: pointer;display: inline-block;float: right;font-size: 11px;font-weight: bold;line-height: 25px;padding: 0 5px;text-align: center;vertical-align: middle;}
ul.right li .icon-next{background: url(../images/global/next.png) no-repeat scroll 0 0 transparent;height: 21px;width: 21px;margin:2px 0 2px 0; opacity: 0.8; text-indent: -99em;overflow:hidden;display:block;}
ul.right li a.page-prev{-moz-transition: all 0.218s ease 0s;background: -moz-linear-gradient(center top , #DDD, #F2F2F2) repeat scroll 0 0 #f9f9f9;box-shadow:0 1px 0 rgba(0, 0, 0, 0.08), 0 18px 2px rgba(255, 255, 255, 0.267) inset, 0 0 2px rgba(0, 0, 0, 0.114) inset, 0 1px 0 #F2F2F2 inset;border-width: 1px;border-style: solid;border-color:#D2D2D2 #C8C8C8 #B1B1B1;border-radius: 3px 0px 0px 3px;color: #666666;cursor: pointer;display: inline-block;float: right;font-size: 11px;font-weight: bold;line-height: 25px;padding: 0 5px;text-align: center;vertical-align: middle;}
ul.right li .icon-prev{background: url(../images/global/prev.png) no-repeat scroll 0 0 transparent;height: 21px;width: 21px;margin:2px 0 2px 0; opacity: 0.25;text-indent: -99em;overflow:hidden;display:block;}

