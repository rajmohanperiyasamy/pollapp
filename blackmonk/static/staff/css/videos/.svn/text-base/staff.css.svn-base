#module-stats a.state-filter-active{-moz-transition: all 0.218s ease 0s;background: -moz-linear-gradient(center top , #FFFFFF, #E9E9E9) repeat scroll 0 0 #f9f9f9;box-shadow:0 1px 0 rgba(0, 0, 0, 0.08), 0 18px 2px rgba(255, 255, 255, 0.267) inset, 0 0 2px rgba(0, 0, 0, 0.114) inset, 0 1px 0 #FFFFFF inset;border-width: 1px;border-style: solid;border-color:#D2D2D2 #C8C8C8 #B1B1B1 #EEE;border-radius: 0px 3px 3px 0px;color: #666666;cursor: pointer;display: inline-block;float: right;font-size: 11px;font-weight: normal;line-height: 20px;padding: 0 5px 2px;text-align: center;vertical-align: middle;}
