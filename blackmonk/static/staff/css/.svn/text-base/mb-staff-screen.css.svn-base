/* -----// Structure \\---- */
* {
	font-size: 100%; 
	padding: 0; 
	margin: 0;
	}
body {
	font: 0.84em/1.333 Arial, sans-serif; 
	margin: 0; 
	padding: 0;
	color: #202020; 
	/*background:url(../img/profile_bar_bg.gif) repeat-x top center;*/
	min-width: 40em; 
	margin: 0 auto;
	}
div.container { 
	width:940px; 
	margin:0 auto;
	text-align:left;
	}
div.main{
	float:left;
	display:block;
	overflow:hidden;
	padding:0;
	margin:0;
	}
div.left_col{
	float:left; 
	width:160px; 
	margin-right:20px; 
	background-color:#f6f6f6;
	margin-top:10px; 
	padding:0 10px 20px 10px;
	-moz-border-radius:4px;
}
div.main_col{
	float:left; 
	width:740px;
	min-height:350px !important;
}

/* -----// Header \\---- */
div#header {
	height:70px;
	}
div#header #mb_logo{
	padding-top:8px;
	width:300px;
	margin-right:20px;
	height:55px;
	float:left;
	display:block;
	outline:none;
	cursor:pointer
	}
div#header #mb_logo a{
	display:block;
	width:201px;
	height:47px;
	cursor:pointer;
	}

#mb_top_nav {
	float:left;
	margin-left:270px;
	height:20px;
	width:350px;
	display:block;
	padding:20px 0;
	font-size:12px;
	text-align:right;
	}
#mb_top_nav .nav_container{
	padding:4px;
	-moz-border-radius:6px;
	display:inline;
	background-color:#1F3253;
	width:200px;
	}	
#mb_top_nav a {
	color:#888;
	text-decoration: none;
	margin-left:1px;
	padding:0 5px;
	}
#mb_top_nav a:hover {
	color: #666;
	}
#mb_top_nav strong{color:#999;}	
#mb_top_nav a.first {
	margin-right:5px;
	display:inline;
	color:#666;
	}

#main_nav{
	padding-top:20px;	
	}

#mb_main_nav{
	width:940px;
	display:block;
	padding:0;
}
#main_nav .nav{
	display:block;
	margin-right:20px;
	width:140px;
	float:left;
	}
#main_nav .nav li { 
	display:block;
	width:140px;
	height:18px;
	line-height:18px;
	padding:1px 0 2px 0;
	}
#main_nav .nav li.on{
	background:#777;
	-moz-border-radius:4px;
}
#main_nav .nav li.on:hover{
	background:#6b6b6b;
}
#main_nav .nav li:hover{
	background:#ddd;
	-moz-border-radius:4px;
}
#main_nav .nav li:hover a{

	color:#222;
}
#main_nav .nav li a{ 
	padding:0 7px;
	font-size:12px;
	_float:left;
	outline:none;
	color:#454545;
	text-decoration:none;
	text-transform:uppercase;
	font-weight:bold;
}	
#main_nav .nav li.on a {
	color:#FFF;
}
/* self-clear floats */
hr {
	clear:both !important;
	background:none;
	border:none;
	height:0px;
	margin:0;
	padding:0;
	visibility:hidden;
	}
.group:after {
	content: "."; 
    display: block; 
    height: 0; 
    clear: both; 
    visibility: hidden;
    }

