.fileupload-content{}
.bm-uploader-list{}

.upload-button-wrapper {position:relative;}
.upload-button {
	background-color: #4D90FE;
	background-image: -moz-linear-gradient(center top , #4D90FE, #4787ED);
	background-image: -webkit-linear-gradient(center top , #4D90FE, #4787ED);
	background-image: -o-linear-gradient(center top , #4D90FE, #4787ED);
	background-image: -ms-linear-gradient(center top , #4D90FE, #4787ED);
	border: 1px solid #3079ED;
	color: #FFFFFF;
	border-radius: 2px 2px 2px 2px;
	cursor: pointer !important;
	font-size: 11px;
	font-weight: bold;
	height: 27px;
	line-height: 27px;
	margin-right: 16px;
	min-width: 54px;
	outline: 0 none;
	padding: 0 8px;
	text-align: center;
	display: inline-block;
	position: relative;
}
.upload-button-wrapper .fileinput-button{
	height: 34px;
    left: 0;
    overflow: hidden;
    position: absolute;
    top: 0;
    width: 98px;
    z-index: 999;
}
.upload-button-wrapper.w160 .fileinput-button, .upload-button-wrapper.w160 .fileinput-button input{
	width:160px;
	height:34px;	
}
*+html .upload-button-wrapper .fileinput-button { 
    top: 0px;
	position:absolute;
}
.upload-button-wrapper .fileinput-button input{
	font-size: 12px;
    height: 29px;
    left: 0;
    opacity: 0;
    position: absolute;
    top: 0;
    width: 80px;
	filter: alpha(opacity = 0);
}
.upload-button-wrapper .fileinput-button input{width:80px\9;}

.bm-uploader-file {
	display:inline-block;
	margin: 12px 5px 12px 0;
	position: relative;
	text-align: center;
	vertical-align: middle;
	*display:block;
	*float:left;
}
.bm-uploader-file span.img_name{
	color: #666666;
    display: block;
    padding-bottom: 3px;
}
.bm-uploader-file .start {
	position:absolute;
	bottom:0;
	left:0;
}
.bm-uploader-file .preview{overflow:hidden; opacity:1;}
.bm-uploader-file .preview-inner{
	height: 85px;
	padding-top: 5px;
	width: 120px;
	//background: url(../images/design/271211bm001.png) repeat scroll 0 0 brown;
	background: none repeat scroll 0 0 #F4F5F7;
	color: rgba(255,255,255, 0.35);
}
.bm-uploader-file.finished .preview-inner{padding-top:0px; height:90px;}
.bm-uploader-file .progress-wrapper {bottom: 3px;left: 0;position: absolute !important;width: 100%; display:inline-block}
.bm-uploader-file .progress{background: none repeat scroll 0 0 transparent; border: medium none; height: 5px;margin: 3px;padding: 0;width: auto;}
.bm-uploader-file .ui-progressbar { height: 100%; text-align: left; }
.bm-uploader-file .ui-progressbar .ui-progressbar-value {background: none repeat scroll 0 0 rgba(2,0,0, 0.6);height:100%; }

.bm-uploader-file p.controls {visibility: hidden;}
.bm-uploader-file:hover p.controls {visibility: visible;}
.bm-uploader-file p.controls {right: 0;margin: 0;padding: 0;position: absolute;bottom: 5px;width: 100%;z-index: 111;}  
.bm-uploader-file small.update {float: right;margin-right: 5px;font-size: 9px;line-height: 11px;text-transform: lowercase;vertical-align: middle;}
.bm-uploader-file small.update a {cursor:pointer;background: url(../ui/images/icons/b/911.png) no-repeat scroll 50% 50% rgba(0,0,0, 0.65);text-indent:-9999em;width:16px;height:15px;line-height:25px;padding:4px;border-radius:50%;color: white;display: inline-block;opacity: 0.7;-moz-transition: all 0.3s ease-in-out;-webkit-transition: all 0.3s ease-in-out;}
.bm-uploader-file small.update a:hover{background-position:50% 50%;opacity: 1;}
.bm-uploader-file small.delete {
	float: right;
	margin-right: 5px;
	font-size: 9px;
	line-height: 11px;
	text-transform: lowercase;
	vertical-align: middle;
}
*+html .bm-uploader-file small.delete { 
    width: 30px;
}
.bm-uploader-file small.delete a {
	cursor:pointer;
	background: url(../images/design/00-delete.png) no-repeat scroll 50% -33px rgba(0,0,0, 0.65);
	background: url(../images/design/00-delete.png) no-repeat scroll 50% -33px transparent\0/;/*only for ie 8*/
	text-indent:-9999em;
	width:16px;
	height:15px;
	line-height:25px;
	padding:4px;
	border-radius:50%;
	color: white;
	display: inline-block;
	*display:block;
	opacity: 0.7;
	-moz-transition: all 0.3s ease-in-out;
	-webkit-transition: all 0.3s ease-in-out;
}
*+html .bm-uploader-file small.delete a   {
	background: url(../images/design/00-delete.png) no-repeat scroll 50% -33px transparent;
}/*only for ie 7*/

.bm-uploader-file small.delete a:hover{background-position:50% 4px;opacity: 1;}

.bm-uploader-file small.cancel {float: right;margin-right: 5px;font-size: 9px;line-height: 11px;text-transform: lowercase;vertical-align: middle;}
.bm-uploader-file small.cancel a {background: url(../images/design/00-delete.png) no-repeat scroll 50% -33px rgba(0,0,0, 0.65);text-indent:-9999em;width:16px;height:15px;line-height:25px;padding:4px;border-radius:50%;color: white;display: inline-block;opacity: 0.7;-moz-transition: all 0.3s ease-in-out;-webkit-transition: all 0.3s ease-in-out;}
.bm-uploader-file small.cancel a:hover{background-position:50% 4px;opacity: 1;}
.dropzone {display:none;width:400px;padding:20px 10px;margin-bottom:10px;background:none repeat 0 0 #f7f7f7;border: 1px solid #EfEfEf;border-radius: 5px;color:#333;}


.bm-uploader-file .preview-inner a{
	display:block;
}
.set-feature .bm-uploader-file .preview.border{
	border:1px solid #FFAE00;
}
.set-feature .bm-uploader-file span.icon-tick{
	display: none;
    left: 0;
    right: auto;
    top: 0;
}
.set-feature .bm-uploader-file span.icon-tick.active{
	display: block;
}











