/* Tipsy */

.tipsy { padding: 5px; font-size: 11px;font-weight:normal; position: absolute; z-index: 99999; }
.tipsy-inner { padding: 3px 8px 4px 8px;background:transparent;filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#85000000,endColorstr=#85000000);zoom: 1; background:none repeat 0 0 scroll black;color: white; text-shadow:0 1px 0 #000; max-width: 200px; text-align: center; }
.tipsy-inner {box-shadow:0 1px 0 rgba(0,0,0, 0.12); /* border-radius:2px; */}
.tipsy-arrow { position: absolute; background: url("../images/global/121211bm002.gif") no-repeat top left; width: 9px; height: 5px;z-index:100000 }
.tipsy-n .tipsy-arrow { top: 0px; left: 50%; margin-left: -4px; }
.tipsy-nw .tipsy-arrow { top: 0; left: 10px; }
.tipsy-ne .tipsy-arrow { top: 0; right: 10px; }
.tipsy-s .tipsy-arrow { bottom: 0; left: 50%; margin-left: -4px; background-position: bottom left; }
.tipsy-sw .tipsy-arrow { bottom: 0; left: 10px; background-position: bottom left; }
.tipsy-se .tipsy-arrow { bottom: 0; right: 10px; background-position: bottom left; }
.tipsy-e .tipsy-arrow { top: 50%; margin-top: -4px; right: 0; width: 5px; height: 9px; background-position: top right; }
.tipsy-w .tipsy-arrow { top: 50%; margin-top: -4px; left: 0; width: 5px; height: 9px; }
.tipsy.ftip .tipsy-inner {background:none repeat 0 0 scroll white !important;color: black !important;}


/* Form */

form fieldset {
	padding:20px 6px;
	margin-bottom:16px;
	position:relative;
}
form fieldset.base_action{
	background:none repeat 0 0 scroll #FFF;
	filter:none;
	text-align:right;
	box-shadow:none;
	}
form div.title/*, div.title */{
	position:relative;
	display:block;
	top:-8px;
	left:-17px;
	margin:0;
	height:35px;
	background:url(../images/global/111211bm001.png) no-repeat 1px 100% transparent;
	color:#B2B2B2;
	/* for IE6/7 */
	*background-position:10px 100%;
}
form div.title h2/*, div.title h2*/ {
	font-weight: bold;
	display:inline-block;
	background:none repeat 0 0 scroll rgb(125,125,125); /* The Fallback */
	background:none repeat 0 0 scroll rgba(0,0,0,0.5);
	color:#FFF;
	font-size:13px;
	margin-right:10px;
	padding:3px 35px 4px 30px;
	min-width:95px;
    /* for IE6/7 */
    *display: inline;
    zoom:1;	
}

form div.title p/*, div.title p*/ {
  color: #aaaaaa;
  display: inline;
}

form div.title p a/*, div.title p a*/ {
  color: #666666;
}

form fieldset.base, form fieldset.divider {
  margin-top: 30px;
  background: #FFFFFF url(../images/design/global/dotted.gif) repeat-x top left;
}

form dl.vertical {
  margin: 0;
}

form dl.horizontal {
  margin: 0;
  padding: 10px 0 0;
}

form dl.base {
  margin: 0;
  padding: 0;
  text-align: right;
}

form dl.horizontal dt {
  clear: both;
  text-align: right;
  float: left;
  white-space: nowrap;
  width: 140px;
  line-height: 1.25em;
}
form dl.horizontal dd {
  margin-left: 150px;
  line-height: 1.35em;
  position:relative
}

form dl.vertical dt {
  clear: both;
  text-align: left;
  white-space: nowrap;
  line-height: 1.25em;
  display: block;
}

form dl.vertical dd {
  position:relative
  line-height: normal;
}

form dl.file dd {
  padding-left: 20px;
}

form dl.vertical dt label, form dl.vertical dd label, form dl.horizontal dt label, form dl.horizontal dd label {
  color: #333;
  font-size: 13px;
  font-weight: normal;
}
form dl.vertical dt label:after, form dl.horizontal dt label:after, form dl.horizontal dd label:after {content:":"; font-weight:bold}
form dl.vertical dt label.after_none:after, form dl.horizontal dt label.after_none:after, form dl.horizontal dd label.after_none:after {content:"" !Important; }
form dl.vertical dt label, form dl.horizontal dt label {
  line-height: 200%;
  margin: 0 0 .2em 0;
}

form dl.horizontal dd label {
  margin: 0 0 .2em 0;
  display: block;
}
form dl.vertical dt label, form dl.vertical dd label {
  margin: 0 0 .2em 0;
  display:block;
 /* width:100%;*/
}
