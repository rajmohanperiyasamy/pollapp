	border-width:1px 1px 1px 0 !important;
}
.account-bar .primary-nav ul > li.modules a.arrow-down,
.account-bar .account-nav ul > li.user-options a.arrow-down{
	background-position:-24px -24px;
}
.account-bar .primary-nav ul > li.divider{
	width:2px;
}
.account-bar .primary-nav ul > li.divider,
.account-bar .account-nav ul > li.divider{
	border-width:1px 0;
	padding:1px 0 0;
}
.account-bar .primary-nav ul > li.divider span,
.account-bar .account-nav ul > li.divider span{
	text-indent:-9999em;
	display:block;
	border-color:transparent #3d66aa transparent #6a92d2;
	border-style:solid;
	border-width:0 1px;	
	line-height:23px;
}
.account-bar .primary-nav ul li:hover .sub-menu{
	z-index: 10000 !important;
    opacity: 1;
    filter:alpha(opacity=100);
	visibility:visible;
  }
.account-bar .primary-nav ul li:hover .sub-menu li{
    height: 20px;
}
.account-bar .primary-nav ul li .sub-menu {
    background:none repeat 0 0 scroll rgba(0,0,0,0.85);
	border:1px solid rgba(0,0,0,1);
	box-shadow:0 0px 3px rgba(0,0,0, 0.6); 
	visibility:hidden;
    opacity: 0;
	z-index: -1;
    filter:alpha(opacity=0);
	white-space: nowrap;
	border-radius:3px;
    height: auto;
    left: 0;
    margin: 0;
    padding:0;
    position: absolute;
    top: 29px;
    width: 200px;
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
	-o-transition: all 0.3s;
	-ms-transition: all 0.3s;
    transition: all 0.3s;	}
.account-bar .primary-nav ul li .sub-menu li {
    background:none repeat 0 0 scroll transparent;
	border:none;
	display:block;
    height: 0;
	color:#FFF;
	box-shadow:none;
	float:none;
	line-height:20px;
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
	-o-transition: all 0.3s;
	-ms-transition: all 0.3s;
    transition: all 0.3s;	
}
.account-bar .primary-nav ul li .sub-menu li:last-child { 
	margin-bottom: 10px; 
}
.account-bar .primary-nav ul li .sub-menu li:nth-child(2) {
	padding-top:10px;
}	
.account-bar .primary-nav ul li .sub-menu li.dropdown-arrow{ 
	position: absolute; background: url("../images/global/121211bm002.gif") no-repeat top left transparent; 
	width: 9px; 
	height: 5px;
	z-index:100000;
	top: -6px; 
	left: 10px; 
	margin-left: -4px; 
}
.account-bar .primary-nav ul li .sub-menu li.divider {
    border-bottom: 1px solid #444;
	background-color:#000;
    height: 0;
	line-height:0;
    margin: 4px 0;
}
.account-bar .primary-nav ul li .sub-menu li a {
    border: medium none;
    font-size: 12px;
    font-weight: normal;
	padding:0 10px;
	color:#EAEAEA;
	background:none repeat 0 0 transparent;
