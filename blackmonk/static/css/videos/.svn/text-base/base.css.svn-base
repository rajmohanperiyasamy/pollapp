/* Reset styles
---------------------------------------------- */

html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp, small, strong, sub, sup, var, 
b, i, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, figcaption, figure, footer, header, hgroup, menu, nav, section, summary, time, mark, audio, video {
	margin:0;
	padding:0;
	border:0;
	font-size:100%;
	font:inherit;
	vertical-align:baseline;
	background:transparent;
}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {display:block}
ol,  ul {list-style: none;}
blockquote, q {quotes:none}
blockquote:before, blockquote:after, q:before, q:after {content:'';content:none}
table {border-collapse: collapse;border-spacing: 0;}


/* Base styles
---------------------------------------------- */

body {
	font:normal 12px/150% 'Helvetica Neue',Helvetica,Arial,sans-serif; 
	color:#232323;
	background:none #FFFFFF;
}

/* --------- links ----------- */

a:link, a:visited {color: #4083a9;outline: none; text-decoration: none;}
a:hover {text-decoration: none;color: #205f82;}

/* -------- Typography ------- */

h1{font-size:24px;font-weight:normal;color:#333}
h2{font-size:20px;font-weight:bold;color:#333}
h3{font-size:18px;font-weight:bold;color:#333}
h4{font-size:14px;font-weight:normal;color:#333}
h5{font-size:14px;font-weight:bold;;color:#333}
h6{font-size:14px;font-weight:normal;color:#333}
p{margin-bottom:1em;line-height:1.5em}
.txt-right{text-align:right}
.txt-center{text-align:center}
.txt-bold{font-weight:bold}
.txt-italic{font-style:italic}
.txt-underline{text-decoration:underline}
.txt-regular{font-weight:normal}
a.small{font-size:11px;}

/* -------- Misc ------- */



/* -------- Site Bar ------- */

.account-bar{
	position: fixed;
	top: 0;
	width: 100%;
	background: none repeat scroll 0 0 #517DC5;
    border-bottom: 1px solid rgba(0, 0, 0, .45);
	box-shadow: 0 0px 3px rgba(0, 0, 0, 0.5);
	height: 52px;
	opacity: 1;
	filter: alpha(opacity=100);
	z-index: 990;
	}
#site-name {margin:0 15px;float:left}	
#site-name h1.account-logo {
    font:bold 24px/175% 'Helvetica',Helvetica,Arial,sans-serif;
	letter-spacing:-1px;
	text-shadow:0px -1px 0 rgba(0,0,0,.2)
}
#site-name h1.account-logo a{
	color:rgba(255,255,255,.975);
	-moz-transition: opacity 0.15s ease 0s;
	-webkit-transition: opacity 0.15s ease 0s;
	-o-transition: opacity 0.15s ease 0s;
	-ms-transition: opacity 0.15s ease 0s;
	}
#site-name h1.account-logo a:hover{
	opacity:0.75;
	}
#site-name h1.account-logo span{
	position:relative;
	padding:5px 0; 
	display:block
	}
#site-name h1.account-logo img{
	display: block;
    max-width: 180px;
	max-height: 42px;
}

.account-bar .primary-nav{float:left;width:200px; padding:12px 0;color:#FFF; margin:0 10px;}
.account-bar .primary-nav ul > li,
.account-bar .account-nav ul > li{
	position: relative;
	display: inline-block;
    vertical-align: top;
	background: #4575c5;
	background: -webkit-gradient(linear, left top, left bottom, from(#5787d5), to(#4575c5));
	background: -moz-linear-gradient(top,  #5787d5,  #4575c5);
	background: -o-linear-gradient(top,  #5787d5,  #4575c5);
	background: -ms-linear-gradient(top,  #5787d5,  #4575c5);
	border-color:#3b63a4 #355c9b #315692;
	border-style:solid;
	border-width:1px;
	color:rgba(255,255,255,0.75);
	box-shadow:0 1px 0px rgba(0,0,0,.08), 0 0px 2px rgba(0,0,0,0.115) inset, 0 1px 0px rgba(255,255,255,0.30) inset;
	-moz-transition: all 0.2s ease-in-out 0s;
	-webkit-transition: all 0.2s ease-in-out; 
	-o-transition: all 0.2s ease-in-out;
	-ms-transition: all 0.2s ease-in-out;
	float:left;
	line-height:24px;
}
.account-bar .primary-nav ul > li:hover,
.account-bar .account-nav ul > li.user-options:hover,
.account-bar .account-nav ul:hover > li.user-options{
	background:#3b66ac;
	background: -webkit-gradient(linear, left top, left bottom, from(#4875bd), to(#3b66ac));
	background: -moz-linear-gradient(top,  #4875bd,  #3b66ac);
	background: -o-linear-gradient(top,  #4875bd,  #3b66ac);
	background: -ms-linear-gradient(top,  #4875bd,  #3b66ac);
	border-color:#3b63a4 #355c9b #315692;
	border-style:solid;
	border-width:1px;
	color:rgba(255,255,255,0.75);
	box-shadow:0 1px 0px rgba(0,0,0,.08), 0 0px 2px rgba(0,0,0,0.115) inset, 0 1px 0px rgba(255,255,255,0.30) inset;
}
.account-bar .primary-nav ul li.home{
	border-radius:3px 0 0 3px;
	border-width:1px 0 1px 1px;
}
.account-bar .primary-nav ul li.home a,
.account-bar .primary-nav ul li.modules a.arrow-down,
.account-bar .account-nav ul li.user-icon .avatar,
.account-bar .account-nav ul li.user-options a.arrow-down{
	padding:6px 8px;
	background-image:url("../images/icons/app_24_sprite.png");
	background-repeat:no-repeat;
	background-color:transparent;
	width:12px; 
	height:12px; 
	text-indent:-9999em; 
	display:block;
	overflow:hidden
}
.account-bar .primary-nav ul li.home a{
	background-position:-24px 0px;
	}
.account-bar .primary-nav ul > li.modules,
.account-bar .account-nav ul > li.user-options{
