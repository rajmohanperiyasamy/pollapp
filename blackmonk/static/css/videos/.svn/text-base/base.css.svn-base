/* -------- Alerts ------- */

.alert-bar {
  	background: url("../images/global/271211bm001.png") repeat scroll left top brown /*#DF3B3B*/;
    border-bottom: 1px solid rgba(0,0,0,.225);
	box-shadow:inset 0px -1px 0px rgba(255,255,255,0.25);
	text-shadow:0 -1px 0 rgba(0, 0, 0, 0.35);	
	min-height:42px;
	opacity:1;
	overflow: hidden;
	position:fixed;
	white-space: nowrap;
	width: 100%;
	z-index: 10;
	-moz-transition: all 0.6s ease-in-out;
	-o-transition: all 0.6s ease-in-out;
	-ms-transition: all 0.6s ease-in-out;
	-webkit-transition: all 0.6s ease-in-out; 		
}
.alert-bar h4 {
	color: #FFFFFF;
    font-size: 14px;
    padding: 12px 0;
}
.alert-bar h4 a{font-weight:bold; color:#FFF; border-bottom:1px dotted rgba(255,255,255,0.75);}

.alert-bar .dismiss {
    opacity: 0.5;
    position: absolute;
    right: 12px;
    top: 12px;
}
.alert-bar .dismiss:hover {
    opacity: 1;
}
.alert-bar .dismiss {
	background:none repeat 0 0 rgba(0,0,0, 0.35);
    color: #FFFFFF;
	font-family:"Trebuchet MS", Arial, Helvetica, sans-serif;
    font-size: 18px;
	line-height:0px;
	padding:8px 4px 10px 3px;
	border-radius:50%;
    font-weight: bold;
    margin: 0;
    text-decoration: none;
	-moz-transition: opacity 0.3s ease-in-out;
	-o-transition: opacity 0.3s ease-in-out;
	-ms-transition: opacity 0.3s ease-in-out;
	-webkit-transition: opacity 0.3s ease-in-out; 	
}

/* -------- Header ------- */

header{
	z-index: 989;
	background: /*url(../images/global/p-gry.png) repeat scroll*/ #E9E9E9;
	border-bottom:1px solid #C2C2C2;
	box-shadow: 0 0px 3px rgba(0, 0, 0, 0.2);
	clear: both;
	min-height: 75px;
	margin-bottom:5px;
	width: 100%;
	-moz-transition: all 0.2s ease-in-out 0s;
	-webkit-transition: all; 
	-webkit-transition-duration : 0.2s; 
	-webkit-transition-timing-function : ease-in-out;
	-o-transition: all; 
	-o-transition-duration : 0.2s; 
	-o-transition-timing-function : ease-in-out; 
	-ms-transition: all; 
	-ms-transition-duration : 0.2s; 
	-ms-transition-timing-function : ease-in-out;  		
	}
