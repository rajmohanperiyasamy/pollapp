/* Zooming effect begin */
/* Caution do not change order of the style of zoomie */
.zoomie_outer{
	width:100%;
	height:100%;
	visibility:hidden;	
}

.ds_image_popup{
	visibility:hidden;
}

.zoomie_content_holder {
	display: none;
	position: absolute;
	top: 15%;
	left: 50%;	
	padding: 16px;	
	border: 5px solid black;
	-moz-border-radius: 1em;
	-webkit-border-radius:1em;
	border-radius: 1em;
	background-color: white;
	z-index:9002;
}

.zoomie_lb_outer{
	padding:6px;
	border:1px solid #bbb;
	background-color:#F4F4F4;
	-moz-border-radius: 1em;
	-webkit-border-radius:1em;
	display:block;
}

.zoomie_close{
	left:-15px;right:auto;
}

#ds_image_parent{
	width:100px;height:100px;overflow:hidden;
}
.zoom_image{
	cursor:pointer;
}

.zoomie_blur{
	background-color: #ccc; /*shadow color*/
	color: inherit;
	margin-left: 4px;
	margin-top: 4px;
}


.zoomie_shadow{
	background-color: #666; /*shadow color*/
	color: inherit;
}

.shadow{
	background: transparent url(images/tm.png) repeat-x scroll 0pt 0pt;
	overflow: hidden;
	-moz-background-clip: -moz-initial;
	-moz-background-origin: -moz-initial;
	-moz-background-inline-policy: -moz-initial;
	height: 20px;
}


/* Zooming effect end */

