	}
.f_event_slide_nav .pages li{
	display:inline;
	}
.f_event_slide_nav .pages li a{
	display:block;
	float:left;
	border-right:1px solid #1a1a1a;
	width:24px;
	height:22px;
	text-align:center;
	font-size:12px;
	line-height:22px;
	font-weight:bold;
	color:#FFF;
	}	
.f_event_slide_nav .pages li a:hover{
	background-color:#4c4c4c;
	}
.f_event_slide_nav .pages li.active a{
	background-color:#090;
	}







