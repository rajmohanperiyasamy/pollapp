.getdirections h3{
color: #666666;
    display: block;
    font-size: 13px;
    font-weight: bold;
    margin-bottom: 16px;
}
.getdirections .travel-modes{
margin-bottom:20px;
}
.getdirections span.location{
display: block;
    margin-bottom: 10px;
}
.getdirections span.location label{
background: none repeat scroll 0 0 #EEEEEE;
    border: 1px solid #C1C1C1;
    border-radius: 100% 100% 100% 100%;
    box-shadow: 0 0 6px 1px rgba(0, 0, 0, 0.2);
    color: #666666;
    font-weight: bold;
    padding: 3px 7px;
    text-shadow: 1px 1px #FFFFFF;
}
.getdirections span.location input{
border: 1px solid #D2D2D2;
    border-radius: 2px 2px 2px 2px;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.15);
    height: 22px;
    padding-left: 5px;
    width: 258px;
}
.getdirections span.getdirections-btn{
display: block;
    margin-bottom: 20px;
    margin-top: 20px;
    padding-right: 2px;
    text-align: right;
}
.travel-modes li {
    display: block;
    float: left;
    font-size: 11px;
    height: 28px;
    line-height: 28px;
    margin-right: 5px;
    overflow: visible;
    text-shadow: 0 1px 0 #FFFFFF;
    width: auto;
}
.travel-modes li a {
    background: -moz-linear-gradient(center top , #FEFEFE, #F7F7F7) repeat scroll 0 0 transparent;
    border-color: #D7D7D7 #DDDDDD #D0D0D0 #D0D0D0;
    border-style: solid;
    border-width: 1px 1px 1px 0;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    cursor: pointer;
    display: block;
    float: left;
    height: 26px;
    line-height: 23px;
    padding: 0 6px;
    position: relative;
    text-align: center;
    width: 27px;
}
.travel-modes li a.active {
    background: none repeat scroll 0 0 #EEEEEE;
    border-color: #C3C3C3;
    box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.1) inset, 0 1px 2px #FFFFFF;
}
.travel-modes li a.first {
    border-left-width: 1px;
    border-radius: 3px 0 0 3px;
}
.travel-modes li a.last {
    border-radius: 0 3px 3px 0;
}
.travel-modes li a span {
    cursor: pointer;
    display: inline-block;
*display:block;
*float:left;
    height: 16px;
    opacity: 0.5;
    text-indent: -9999em;
    vertical-align: middle;
    width: 16px;
}
.travel-modes li a:hover span {
    opacity: 0.85;
}
.travel-modes li a.active span {
    opacity: 0.85;
}
.travel-modes li a.travel-by-car span {
    background: url("../images/global/map-icon.png") no-repeat scroll 0 0 transparent;
    margin-top: 7px;
height:13px;
}
.travel-modes li a.travel-by-walking span{
background: url("../images/global/map-icon.png") no-repeat scroll 0 -31px transparent;
    height: 16px;
    margin-top: 5px;
}
.travel-modes li a.travel-by-bus span{
background: url("../images/global/map-icon.png") no-repeat scroll 0 -14px transparent;
    height: 16px;
    margin-top: 6px;
}
.travel-modes li a.travel-by-bicycling span{
   background: url("../images/global/map-icon.png") no-repeat scroll 0 -48px transparent;
   height: 16px;
   margin-top: 5px;
       width:19px;
}

.getdirections .directions-result{
display: block;
