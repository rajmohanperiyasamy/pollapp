    -moz-border-radius: 3px;
-khtml-border-radius: 3px;
-webkit-border-radius: 3px;
    background: #000 url(http://bm.static.s3.amazonaws.com/themes/green/images/global/small_gallery.gif) no-repeat center center;
}


