#deal_info ul li#number_bought #progressbar{}
#deal_info ul li#number_bought .widget_content {
	-moz-background-clip:border;
	-moz-background-inline-policy:continuous;
	-moz-background-origin:padding;
	background:#e6e6e6;
	color:#222222;
	font-size:8px; /*fix ie*/
}
#deal_info ul li#number_bought .progressbar {
	height:10px;
	text-align:left;
	margin:3px 0 4px;
	/*border:1px solid #c75232;*/
}
#deal_info ul li#number_bought #progressbar .progressbar_value {
	height:100%;
	/*background-image: url(../images/global/pbar-ani.gif);*/
}
#deal_info ul li#number_bought .widget_header {
	-moz-background-clip:border;
	-moz-background-inline-policy:continuous;
	-moz-background-origin:padding;
	background:#64d312 /*ec6d28*/;
	border-right:2px solid #3d9f02;
	color:#222222;
	font-weight:bold;
}
#deal_info ul li#number_bought #progressbar .up_done{border-right:none !Important}
#deal_info ul li#number_bought .get_deal{font-size:11px; color:#3d3d3d;padding-top:10px;}
#deal_snap .todays_deal {
color:#64B2DF;
}
#deal_snap span.on {
display:block;
}
