#deal_text ul.note{padding:0px 20px 20px 30px;}
#deal_text ul.note li{ list-style:outside decimal;padding:3px 0;line-height:14px;}
.text_items{margin:0 0 12px 0;display:block; padding:0 10px 0 20px;}
.text_items h5{font-size:14px; font-weight:bold; text-transform:uppercase; margin-bottom:8px;}
.text_items ul{margin:0 0 20px 0;}
.text_items li{float:none;margin:0;padding:5px 0 8px 19px; line-height:14px;width:auto;-moz-background-clip:border;-moz-background-inline-policy:continuous;-moz-background-origin:padding;background:transparent url(../images/ico/doc_16.png) no-repeat scroll 0 5px;border-bottom:1px solid #EEEEEE;font-size:11px;}
.text_items li a{color:#4584a9;}
.text_items li a:hover{color:#154c6d}


#subscribe_nav_right{padding:10px; background-color:#fcfcfc; border:1px solid #e6e6e6; -moz-border-radius:6px;margin-top:10px;}
#subscribe_nav_right span{font-size:11px;line-height:24px; text-transform:uppercase; font-weight:normal;color:#666;float:left;padding-right:18px;}
#subscribe_nav_right a{float:left; display:block; width:24px; height:24px; background:transparent url(../images/global/deal_bar.png) no-repeat scroll 0 0; overflow:hidden;text-indent:-999px; cursor:pointer; margin-right:5px;}
#subscribe_nav_right a.fb_icon{background-position:-11px -263px}
#subscribe_nav_right a.tw_icon{background-position:-45px -263px}
#subscribe_nav_right a.bz_icon{background-position:-79px -263px}
#subscribe_nav_right a.rs_icon{background-position:-113px -263px}

#deal_for_business{padding:10px; background-color:#fcfcfc; border:1px solid #e6e6e6; -moz-border-radius:6px;margin-top:10px;}
#deal_for_business h2{font-size:16px; font-weight:bold; margin-bottom:4px;}
#deal_for_business p{font-size:12px;line-height:15px; }
#deal_for_business p a{color:#06C}

#how_it_work_steps{padding:0;border:1px solid #e8e8e8;background-color:#fcfcfc;-moz-border-radius:12px;}
#how_it_work_steps ul{padding:0; margin:0;}
#how_it_work_steps ul li{list-style:none none;float:left;width:166px;height:280px;border-right:1px solid #eee;padding:0 12px 12px 12px;}
#how_it_work_steps ul li p{font-size:13px; line-height:17px; letter-spacing:-.01em; text-align:center}
#how_it_work_steps ul li span{display:block;text-indent:-9999px;height:50px; margin:10px 0 0; padding:20px 0 0px 0;line-height:50px; overflow:hidden;background:transparent url(../images/global/deal_bar.png) no-repeat scroll 57px -296px}
#how_it_work_steps ul li h5{font-size:22px;font-weight:normal;display:block; letter-spacing:-.01em; text-align:center; line-height:24px;}

#how_it_work_steps ul li#how_to_1 span{background-position:57px -296px}
#how_it_work_steps ul li#how_to_2 span{background-position:57px -376px}
#how_it_work_steps ul li#how_to_3 span{background-position:57px -457px}
#how_it_work_steps ul li#how_to_4 span{background-position:57px -538px}
#how_it_work_steps ul li#how_to_5 span{background-position:57px -618px}

#item_details{background-color:#f0f3f7;padding:10px;-moz-border-radius:6px;-webkit-border-radius:6px;-khtml-border-radius:6px;border-radius:6px;border:1px solid #e4e9f1; margin-bottom:20px;}
table.order_lst{background-color:#fdfdfe;width:100%;margin:10px 0 0 0;-moz-border-radius-bottomleft:5px;-moz-border-radius-bottomright:5px;}
table.order_lst th, table.order_lst td{padding:8px 12px;}
table.order_lst th{
		background:-moz-linear-gradient(center top , #f8f9fb, #e1e6f0) repeat scroll 0 0 transparent;
		background: -webkit-gradient(linear, 0% 10%, 0% 70%, from(#f8f9fb), to(#e1e6f0));
		border:1px solid #d4dde7;
		color:#000;
		cursor:default;
		outline:medium none;
		padding:3px 8px;
		text-align:center;
		vertical-align:middle;
		white-space:nowrap;
		font-weight:bold;
	}
table.order_lst td{font-size:13px; font-weight:bold;border:1px solid #e0e5ef;}	
table.order_lst .ftr td{font-size:16px;border-top:1px solid #e0e5ef}
table.order_lst .ftr td.total_you_pay{text-align:right; font-size:16px;font-weight:bold;color:#1d2739}

.payment_tip{background-color:#acd0ee;-moz-border-radius:6px;border:4px solid #9dc5e8;color:#203a4e;line-height:20px; font-size:16px; letter-spacing:;font-weight:bold;margin-bottom:10px;}
.payment_tip p{display:block; border:2px dashed #FFF;-moz-border-radius:6px;padding:8px 8px 8px 48px;background:transparent url(../images/global/deal_bar.png) no-repeat scroll 6px -698px;}


.tips_block h3 {
    color: #000000;
    font-size: 16px;
    font-weight: bold;
    margin-bottom: 5px;
}
.tips_inner h4 {
    color: #3D3D3D;
}
.tips_inner p {
    color: #666666;
    margin-bottom: 6px;
}