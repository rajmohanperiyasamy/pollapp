#deal_info ul li h4{font-size:14px;display:block;font-weight:normal; margin-bottom:10px; padding-left:28px;color:#0281ca}

#deal_info ul li#value {
overflow:hidden;
padding:10px 0 8px 20px;
_height:72px;
}
#deal_info ul li#value h4{background:url(../images/ico/featured1.png) no-repeat 0 0;}
#deal_info ul li#value dl {
display:block;
text-align:left;
float:left;
width:92px;
}
