text-align:left;
}
#deal_info ul li#value dl dt {
font-size:18px;
line-height:20px;
font-weight:normal;
letter-spacing:-.03em;
/*color:#64B2DF;*/
}
#deal_info ul li#value dl dd {
font-size:12px;
line-height:12px;
color:#777;
display:block;
letter-spacing:-.03em;
padding-bottom:10px;
}
#deal_info ul li#value dl.discount {
	color:#F39;
	width:62px;
}


#deal_info ul li#time_counter {
overflow:hidden;
padding:10px 10px 6px 18px ;
color:#D30E13;
_height:80px;
}
