font-size:9px;
letter-spacing:-.02em;
color:#777;
text-transform:uppercase;
}
#deal_info ul li#time_counter dl dt, #deal_info ul li#time_counter dl dd {
text-align:left;
}

#deal_info ul li#number_bought {
overflow:hidden;
padding:10px 10px 6px 18px ;
/*color:#D30E13;*/
}
