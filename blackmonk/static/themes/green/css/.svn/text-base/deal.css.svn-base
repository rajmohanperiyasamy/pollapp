#deal_hdr_bar ul.nav li a.active{background:url(../images/global/deal_bar.png) no-repeat center -136px;color:#C60}
#deal_hdr_bar #nav_right{display:block;float:right; width:228px; background:transparent url(../images/global/deal_bar.png) no-repeat right -34px;overflow:hidden; height:34px;line-height:34px;}
#deal_hdr_bar ul.nav li.prom{}
#deal_hdr_bar ul.nav li.prom a{margin-left:5px;border:none;line-height:24px;margin-top:5px; background-color:#333; -moz-border-radius:4px; color:#FFF;}
#deal_hdr_bar ul.nav li.prom a:hover{background-color:#C60;}

#subscribe_nav li{display:inline;}
