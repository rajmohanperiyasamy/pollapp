#deal_info ul li#time_counter dl {
display:block;
float:left;
text-align:center;
width:45px;
padding-left:8px;
-moz-border-radius:3px;-webkit-border-radius:3px;-khtml-border-radius:3px;border-radius:3px;;
border-left:1px solid #d6d6d6;
}
#deal_info ul li#time_counter dl dt {
font-size:26px;
line-height:26px;
font-weight:normal;
letter-spacing:-.06em;
text-shadow: #FFF 0px 2px 0px;
}
