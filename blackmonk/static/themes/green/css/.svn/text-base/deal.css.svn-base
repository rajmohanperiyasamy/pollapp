/* CSS Document */


#deal_hdr_bar{height:34px; background:#FFF url(../images/global/deal_bar.png) repeat-x 0 0;overflow:hidden; margin-bottom:20px;}
#deal_hdr_bar h4{display:block; text-indent:-9999px;float:left; width:138px; background:transparent url(../images/global/deal_bar.png) no-repeat 0 -68px;overflow:hidden; height:34px;}
