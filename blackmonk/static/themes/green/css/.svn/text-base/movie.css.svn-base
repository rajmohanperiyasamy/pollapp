#thick_pagination {
	-moz-background-clip:border;
	-moz-background-inline-policy:continuous;
	-moz-background-origin:padding;
	-moz-border-radius-bottomleft:0.4em;
	-moz-border-radius-bottomright:0.4em;	
	background:#efefef;
	height:40px;
	border-top:1px solid #e6e6e6;
}	
