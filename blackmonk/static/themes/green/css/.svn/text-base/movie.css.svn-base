/* Featured Story [Movie] */

.f_story_slide {
    float:left;
    width:362px;
	background:#000;
}
.f_story_summary {
    height:32px;
    overflow:hidden;
}
.f_story_summary a:link,
.f_story_summary a:visited {
    color:#FFF;
}
.f_story_summary h3 a:link,
.f_story_summary h3 a:visited {
    color:#FFF;
}
.f_story_summary a:hover,
.f_story_summary h3 a:hover {
    color:#2ea3ca;
}
.f_story_summary h3 {
	font-weight:bold;
    font-size:16px;
	line-height:22px;
    margin:0 0 0 0;
	text-shadow: rgba(0,0,0,1) 0 1px 0;
}
.f_story_summary p {
    font-size:11px;
	line-height:14px;
    margin:4px 0 8px 0;
    color: #FFF;
	text-shadow: rgba(0,0,0,1) 0 1px 0;
}
.f_story_summary_inner {
    padding:4px 8px;
}
.f_story_slide_inner {
    position:relative;
    overflow:hidden;
    width: 362px;
}
.f_story_slide_inner .f_story_summary {
    position:absolute;
    top:190px;
    left:0;
    z-index:1;
 	background-color: rgba(0,0,0,.88);
	background-color:#000 !important;
    filter:alpha(opacity=70);
    -moz-opacity:.88;
    opacity:.88;
    width:362px;
}
.f_story_slide_inner .f_story_summary:hover {
 	background-color: rgba(0,0,0,1);
    filter:alpha(opacity=1);
    -moz-opacity:1;
    opacity:1;	
	}
.f_story_slide_inner .f_story_summary a.full_story:hover{
	color:#1788c2; 
	}	
.f_story_slide_image {
    display: block;
    width: 362px;
    _width:360px;
    height: 246px;
	background-color:#000;
}
.f_story_slide_nav {
	position:absolute;
	top:222px;
	width:100%;
}
.f_story_slide_nav .prev, .f_story_slide_nav .next {
	-moz-background-clip:border;
	-moz-background-inline-policy:continuous;
	-moz-background-origin:padding;
	background:transparent url(../images/btn/black_arrow_left_24.gif) no-repeat 0 0;
	cursor:pointer;
	display:block;
	float:left;
	height:24px;
	margin:0;
	width:24px;
	text-indent:-9999px;
}
.f_story_slide_nav .next {
	-moz-background-clip:border;
	-moz-background-inline-policy:continuous;
	-moz-background-origin:padding;
	background:transparent url(../images/btn/black_arrow_right_24.gif) no-repeat 0 0;
	float:right;
}
.f_story_slide_nav .prev:hover, .f_story_slide_nav .next:hover {
	background-position:0 -24px; 
	}
.f_story_slide_nav .pages{
	float:left;
	height:22px;
	width:312px;
	display:block;
	padding:0;
	margin:0;
	border:1px solid #1a1a1a;
	}
.f_story_slide_nav .pages li{
	display:inline;
	}
.f_story_slide_nav .pages li a{
	display:block;
	float:left;
	border-right:1px solid #1a1a1a;
	width:24px;
	height:22px;
	text-align:center;
	font-size:12px;
	line-height:22px;
	font-weight:bold;
	color:#FFF;
	}	
.f_story_slide_nav .pages li a:hover{
	background-color:#4c4c4c;
	}
.f_story_slide_nav .pages li.active a{
	background-color:#090;
	}

/* Slider */

.m_slider_page_nav { 
	position:absolute; 
	top:20px; 
	right:60px; 
	height:8px; 
	padding:0; 
	margin:0 auto; 
	text-align:center;
	width:60px; 
	z-index:9999; 
	}
*html .m_m_slider_page_nav {	
	width:38px;
	left:314px;
	}
.m_slider_page_nav li { 
	display:inline; 
}
.m_slider_page_nav li a { 
	display:block; 
	float:left; 
	width:4px; 
	height:4px; 
	padding:2px; 
	margin:0 2px; 
	cursor:pointer; 
	text-indent:-9999px; 
	background:url(../images/global/slider_dot.png) center top no-repeat; 
	/*_background-image:url(../images/global/slider_dot.gif);*/ 
	overflow:hidden; 
}
.black .m_slider_page_nav li a { 
	background-image:url(../images/global/slider_dot_black.png); 
	/*_background-image:url(../images/global/slider_dot_black.gif);*/ 
	}
.m_slider_page_nav li a:hover { 
	background-position:center -8px; 
	}
.m_slider_page_nav li a.active { 
	background-position:center -16px; 
	cursor:default; 
	}
*html .m_slider_page_nav li a.active:hover {
	background-position:center -16px;
	}




