.title_bar {padding-bottom:4px;margin-bottom:10px; position:relative; border-bottom:1px solid #e6e6e6;}
.title_bar .ns {display:block;position:absolute;top:10px;right:0px;width:120px; letter-spacing:.02em;height:22px;background-color:#000; color:#FFF;font-size:14px; text-transform:uppercase; font-weight:normal; line-height:22px; text-align:left; padding:0}
.title_bar .ns .red{background-color:#C30; color:#fff;padding:3px 5px;}
.title_bar .title{font-size:28px;line-height:30px;font-weight:normal; color:#3d3d3d}
.title_bar .sub_title {font-size:11px; color:#999; font-weight:bold;}
.title_bar .sub_title .release_date{padding-right:6px;border-right:1px solid #d7d7d7;margin-right:6px;}
							
#showtimes {}
#showtimes .theatres { margin:0px;padding:0;list-style:none;}
#showtimes .theatres .phone{color:#4e9bda;font-weight:bold;padding-left:5px; font-size:11px; line-height:12px;}
#showtimes .theatres .address{font-size:11px;color:#666;font-weight:normal}
#showtimes .theatres li {display:block;padding:8px; position:relative;_ position:static;border-bottom:1px solid #EEEEEE; background-color:#FCFCFC}
#showtimes .theatres li.alt { background-color:#FFF;} 
#showtimes .theatres li a {}
*html #showtimes .theatres li a{padding-top:0px;}
#showtimes .theatres li a:hover {/*border-color:#ccc;*/}
#showtimes .theatres li h4{font:bold 12px/15px arial, Helvetica, Utkal, sans-serif;margin-top:2px;*margin-top:4px;clear:both; margin-bottom:0px;}
#showtimes .theatres li h4 a{}
#showtimes .theatres li h4 a:hover {color:#000}
#showtimes .theatres li.last {margin-bottom:6px;}
#showtimes .theatres li .tickets{position:absolute;right:10px; top:18px;}
#showtimes .theatres li .tickets span{padding:3px 8px 5px 8px;font-size:10px;}
#showtimes .theatres li:hover {background-color:#F2F7FD}
.showtimes_list {clear:both;color:#666;padding:4px 0px 0px; text-transform:uppercase; font-size:11px; font-weight:normal}
.showtimes_list span{display:inline;margin-right:8px;}
.showtimes_list span a{/*color:#C00*/}
.showtimes_list span a:hover{}
#showtimes .theatres .sms_showtime{position:absolute;right:30px; top:46px;display:none}
#showtimes .theatres li:hover .sms_showtime{display:block}

#movie_photos .photos{padding:10px 9px 7px 6px; background-color:#FFF;border-bottom:1px solid #e6e6e6;}
#movie_photos .photos li{float:left}
#movie_photos .photos img{margin:0 0 3px 3px;display:block;width:80px; height:80px; background-color:#ccc/*padding:1px;border:4px solid #EFEFEF; background-color:#CDCDCD;-moz-border-radius:0px;*/}

#critic-reviews .display_ct_review {padding:24px 10px 14px;font-size:13px;line-height:16px;background-color:#FFF;border-bottom:1px solid #e6e6e6}
#critic-reviews .display_ct_review p{padding:4px 0 8px 0;}
.display_ct_review .ct_title{font-size:18px; font-weight:bold; margin-bottom:10px;text-indent:-9999px;display:block; height:30px;padding:0}
/*.display_ct_review #nytimes{background:url(../images/samples/nytlogo153x23.gif) no-repeat 0 center;}
.display_ct_review #rediff{background: url(../images/samples/reddiff.gif) no-repeat 0 center;}	
*/
.display_ct_review .sub_title{font-size:11px;color:#666; padding-bottom:6px;}
.display_ct_review .ct_foot{padding:6px 0;}
.display_ct_review .ct_foot .ct_rights{color:#808080}

