.bm_alert {
    background-color: #FFFFFF;
    border-color: #CCCCCC #CCCCCC #C1C1C1;
    border-style: solid;
    border-width: 1px;
    box-shadow:0 3px 4px -2px rgba(0, 0, 0, 0.12), 0 1px 4px 3px rgba(0, 0, 0, 0.075);
    color: #222222;
    outline: 0 none;
z-index:999;
overflow:hidden;
}
.bm_alert table TD {
    padding:  24px 20px;
}
.ui-helper-clearfix:after {
    clear: both;
}
.ui-helper-clearfix:before, .ui-helper-clearfix:after {
    content: "";
    display: table;
}
.bm_alert .ui-dialog-titlebar {
    padding: .7em 1.5em;
    position: relative;
}
.bm_alert .ui-dialog-titlebar a {
    display: block;
    overflow: hidden;
    text-indent: -99999px;
}
.ui-widget-header {
background: none repeat scroll 0 0 #F9F9F9;
    border-bottom: 1px solid #E0E0E0;
    color: #333333;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 16px;
    font-weight: bold;
    padding: 12px 20px;
display:none;
}
.ui-widget-header a {
    color: #222222;
}
.bm_alert .ui-dialog-title {
    float: left;
    margin: 0.1em 16px 0.1em 0;
}
.bm_alert .ui-dialog-content {
    background: none repeat scroll 0 0 #FFFFFF;
    overflow: auto;
    position: relative;
}
.ui-widget-overlay {
    background: none repeat scroll 0 0 #FFFFFF;
    height: 100%;
    left: 0;
    opacity: 0.6;
    position: absolute;
    top: 0;
    width: 100%;
}
.bm_alert .ui-dialog-buttonpane {
    margin: 0;
    padding:0px 10px 20px;
    text-align: left;
}
.bmalert-activebtn {
    background: -moz-linear-gradient(center top , #FFFFFF, #E9E9E9) repeat scroll 0 0 transparent;
    border-color: #D2D2D2 #C8C8C8 #B1B1B1;
    border-radius: 3px 3px 3px 3px;
    border-style: solid;
    border-width: 1px;
    box-shadow: 0 1px 0 rgba(0, 0, 0, 0.08), 0 18px 2px rgba(255, 255, 255, 0.267) inset, 0 0 2px rgba(0, 0, 0, 0.114) inset, 0 1px 0 #FFFFFF inset;
    color: #999999;
    cursor: pointer;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 12px;
    font-weight: bold;
    line-height: 13px;
    margin: 0;
    overflow: visible;
    padding: 5px 11px 4px;
    text-transform: none;
    width: auto;
}
.ui-button :hover{border-color: #B2B2B2 #A8A8A8 #919191;
    color: #666666;
    text-decoration: none;}
.bm_alert .ui-dialog-buttonpane .ui-dialog-buttonset {
text-align:center;
}
.bm_alert .ui-state-focus {
background-color: #C43737;
    background-image: -moz-linear-gradient(center top , #D03E3E, #C43737);
    border-color: #AC2A2A #971B1B #971B1B #AD2727;
    border-style: solid;
    border-width: 1px;
    box-shadow: 1px 1px 0 rgba(255, 255, 255, 0.25) inset, -1px -1px 0 rgba(0, 0, 0, 0.1) inset, 1px 1px 0 rgba(0, 0, 0, 0.15);
    color: #FFFFFF;
cursor: pointer;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 12px;
    font-weight: bold;
    line-height: 13px;
    margin: 0;
    overflow: visible;
    padding: 5px 11px 4px;
    text-transform: none;
    width: auto;
border-radius:3px;
margin-right:10px;
}
.bm_alert .ui-state-focus:hover {
   background-color: #B13131;
    background-image: -moz-linear-gradient(center top , #D03E3E, #B13131);
    border-color: #971B1B #821616 #821616 #971B1B;
    box-shadow: 1px 1px 1px rgba(255, 255, 255, 0.3) inset, -1px -1px 0 rgba(0, 0, 0, 0.1) inset, 1px 1px 1px rgba(0, 0, 0, 0.35);
    color: #FFFFFF;

}






