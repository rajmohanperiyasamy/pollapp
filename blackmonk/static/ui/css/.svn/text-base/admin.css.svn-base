.left-nav-container {
    border-bottom: 1px solid #DFDFDF;
    border-top: 1px solid #FFF;
    background: -moz-linear-gradient(center top , #F5F5F5 0pt, #F0F0F0 100%) repeat scroll 0 0 #F2F2F2;
    -moz-transition: border  0.2s ease 0s;
    border-right: 4px solid #f2f2f2;
    display:block
}
a.left-nav .icon {
    margin-right: 6px;
    padding: 3px 0;
}
a.left-nav, a.left-nav:link, a.left-nav:visited {
    color: #444444;
    cursor: pointer;
    display: block;
    font-family: segoe ui,arial,helvetica,sans-serif;
    font-size: 14px;
    line-height: 27px;
    margin: 0;
    min-height: 34px;
    padding: 0 5px 0 13px;
