.left-nav .nav-collaps,
.left-nav .nav-expand{
	display:block; 
	position:absolute;
	width:12px; 
	height:12px; 
	right:0px; 
	top:0px;
	color:#aaa;
	font-size:22px;
	text-shadow: -1px 0 0 #FFFFFF;
	font-weight:normal;
}
.left-nav .nav-expand{
	-webkit-transform: rotate(90deg);
	-moz-transform: rotate(90deg);
	-ms-transform: rotate(90deg);
	-o-transform: rotate(90deg);
	filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);
	top:12px;
	right:-7px;
}

.left-nav-container:hover {
    background: -moz-linear-gradient(center top , #F0F0F0 0pt, #EFEFEF 100%) repeat scroll 0 0 #EEE;
    border-right-color: #666666;
    text-decoration: none;
}
.left-nav-container.selected-child, .left-nav-container.selected-child:hover,
.left-nav-container.selected, .left-nav-container.selected:hover {
    background: none repeat scroll 0 0 #F9F9F9;
    border-right-color: brown;
}
.left-nav-container:hover .left-nav, 
.left-nav-container.selected .left-nav,
.left-nav-container.selected-child .left-nav,
.left-nav-container.selected-child:hover .left-nav, 
.left-nav-container.selected:hover .left-nav {
	color:brown;
}
.left-nav-container.parent {
    background: -moz-linear-gradient(center top , #EFEFEF, #DDD) repeat scroll 0 0 #F4F4F4;
    border-right:none;
