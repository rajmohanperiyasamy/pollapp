/* admin css */


body.admin {
	padding-top: 52px;
	font-family:segoe ui, arial, helvetica, sans-serif;		
	}
body.admin .tips_block{padding:12px;}	
body.admin #content_inner, .main_wraper{
	width: 984px;
	padding-top:25px;
	}	
body.admin #admin_wrapper{
	background:#F7F7F7;
	border-width:1px;
	border-style:solid;	
    border-color:#D1D1D1 #D1D1D1 #C2C2C2;
    border-radius: 5px 5px 5px 5px;
    position: relative;
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
}
#admin-tab{
	-moz-transition: all 0.4s ease-in-out 0s;
	-webkit-transition: all; 
	-webkit-transition-duration : 0.4s; 
	-webkit-transition-timing-function : ease-in-out; 		
	}	
#admin-tab ul.admin-tab-items {
    height: 85px;
    overflow: hidden;
    padding:5px 0 0;
	margin-bottom:-5px;
}
#admin-tab ul.admin-tab-items li {
    border-radius: 5px 5px 5px 5px;
    cursor: pointer;
    float: left;
    font-size: 11px;
    height: 85px;
    margin: 0px 5px 0 0;
    position: relative;
    text-align: center;
    white-space: nowrap;
    width:auto;
}
#admin-tab ul.admin-tab-items li a {
	color: #777;
    display: block;
    height: 56px;
	padding:18px 12px 0;
    line-height: 24px;
    text-align: center;
    text-decoration: none;
	-moz-transition: all 0.2s ease-in-out 0s;
	-webkit-transition-property : all; 
	-webkit-transition-duration : 0.2s; 
	-webkit-transition-timing-function : ease-in-out; 
}
#admin-tab ul.admin-tab-items li a:hover{
	padding-top:13px;
	height: 61px;
	color:#000;
/*	background: #f6f6f6;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#E9E9E9', endColorstr='#FFFFFF');
	background: -webkit-gradient(linear, left top, left bottom, from(#E9E9E9), to(#FFFFFF));
	background: -moz-linear-gradient(top,  #E9E9E9,  #FFFFFF);
*/	}
#admin-tab ul.admin-tab-items li a:hover .icon{
	opacity:0.75;
}
#admin-tab ul.admin-tab-items li.active a, 
#admin-tab ul.admin-tab-items li.active a:hover{
 	background: -moz-linear-gradient(center top , #FCFCFC, #F1F1F1) repeat scroll 0 0 transparent;
	border: 1px solid #DADADA;
	border-radius: 6px 6px 0 0;
	box-shadow: 0 1px 0 #FFFFFF inset;
	color: #000000;
	margin-top: 5px;
	}
#admin-tab ul.admin-tab-items li.active a:hover{ height:56px; padding-top:18px; margin-top:5px; cursor:default}	
#admin-tab ul.admin-tab-items li.active a .icon{
	opacity:0.6;
	margin-top:-5px;
	}	
#admin-tab ul.admin-tab-items li .icon {
    display: block;
    height: 28px;
    width: 28px;
    margin: 0 auto;
	opacity:0.6;
}
#admin-tab ul.admin-tab-items li .title {
    display: block;
    font-size: 16px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
#admin-tab ul.admin-tab-items .dashboard .icon {
	background:url(../images/icons/a/53-house.png) no-repeat 50% transparent;
}
#admin-tab ul.admin-tab-items .config .icon {
	background:url(../images/icons/a/Settings.png) no-repeat 50% transparent;
}
#admin-tab ul.admin-tab-items .usermgmt .icon {
	background:url(../images/icons/a/usermgmt.png) no-repeat 50% transparent;
}
#xadmin-tab ul.admin-tab-items .config .icon {
	background:url(../images/icons/a/20-gear2.png) no-repeat 50% transparent;
}
#admin-tab ul.admin-tab-items .api_configuration .icon {
	background:url(../images/icons/a/api_32.png) no-repeat 50% transparent;
}
#admin-tab ul.admin-tab-items .design .icon {
	background:url(../images/icons/a/98-palette.png) no-repeat 50% transparent;
}
#admin-tab ul.admin-tab-items .report .icon {
    background:url(../images/icons/a/report-icon.png) no-repeat 50% transparent;
}

#admin-platform .header-bar{
	background: #F3F3F3;
	background: -webkit-gradient(linear,left top,left bottom,from(#FCFCFC),to(#F1F1F1));
	background: -moz-linear-gradient(top,#FCFCFC,#F1F1F1);
	background: -o-linear-gradient(top,#FCFCFC,#F1F1F1);
	border-bottom: 1px solid #C2C2C2;
    border-radius: 5px 5px 0 0;
    height: 42px;
    position: relative;
    text-shadow: 1px 1px 1px #FFFFFF;
	box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);
	z-index:11;
}
#admin-platform .header-bar h3{
	float:left;
	display:block;
	font-size:16px;
	line-height:42px;
	width:136px;
	border-right:1px solid #CFCFCF;
	padding-left:30px; 
	background:url(../images/icons/a/20-gear2.png) no-repeat 0 50% transparent;
}
#nav-roll {
	display:block;
	color:#777;
}
#nav-roll li{
	display:inline
}
#nav-roll li b,
#nav-roll li a,
#nav-roll li a:link,
#nav-roll li a:visited {
	color: #666666;
	display: inline-block;
	font-size: 14px;
	padding: 11px 40px 13px 42px;
	position: relative;
	margin-left:-22px;
	text-decoration: none;
	vertical-align: top;
}
#nav-roll li.first b,
#nav-roll li.first a,
#nav-roll li.first a:link,
#nav-roll li.first a:visited {
	padding: 12px 40px 12px 50px;
	border-radius:5px 0 0 0;	
	margin-left:0;
	font-size:14px;
	text-transform:uppercase;
	font-weight:bold;
	color:#212121;
	width:106px;
}
#nav-roll li#users-index b{
	padding-left:20px;
	width:60px;
}
#nav-roll li .icon{
	position:absolute;
	left:12px;
	top:8px;
	width:28px; 
	height:28px;
	display:inline-block;
	opacity:0.9;
	}
#nav-roll li#modules-index .icon{
	background:url(../images/icons/b/34.png) no-repeat 50% #FFF;
	border:1px solid #e6e6e6;
	border-radius:4px;
	top:6px;
	left:10px;
}
#nav-roll li#pages-index .icon {
	background:url(../images/icons/a/180-stickynote.png) no-repeat scroll 50% 50% #FFFFFF;
    border: 1px solid #E6E6E6;
    border-radius: 4px 4px 4px 4px;
    left: 10px;
    top: 6px;
}
#nav-roll li#config-index .icon{
	background:url(../images/icons/a/20-gear2.png) no-repeat 50% transparent;
}
#nav-roll li b,
#nav-roll li a,
#nav-roll li.active:hover a,
#nav-roll li.active:active a{
	background:url(../images/global/060112bm-roll.png) no-repeat 100% -1px transparent;
}
#nav-roll li:hover a {
	background:url(../images/global/060112bm-roll.png) no-repeat 100% -51px transparent;
	color:#000000;
}
#nav-roll li:active a,
#nav-roll li:active b {
	background:url(../images/global/060112bm-roll.png) no-repeat 100% -1px transparent;
}
#nav-roll li.active a,
#nav-roll li.active b{
	color:#808080;
	opacity:0.65;
	cursor:default;
	}


#core-wraper{    
	border-radius:5px;
	overflow: hidden;
}
#core-wraper #aside{
    background: none repeat scroll 0 0 #F7F7F7;
    box-shadow: 1px 0 2px rgba(0, 0, 0, 0.1) inset;	
    border-radius: 0 0 5px 5px;
    float: left;
    right: 0;
    margin-bottom: -25000px;
    padding-bottom: 25000px;
	margin-right:0px;
}
#core-wraper .admin-main{
    background: none repeat scroll 0 0 #FEFEFE;
    margin-bottom: -25000px;
    min-height: 550px;
    overflow: hidden;
    padding-bottom: 25000px;
	border-right:1px solid #CCC;
	width:743px;
	margin-right:0px;
	position:relative	

}
#core-wraper .admin-main.full_width{
	width:982px;
	border-right:none;
}
#core-wraper .admin-main.full_width .col_9 {
    width: 796px;
}
#core-wraper .admin-main .left-nav-list{ z-index:4;position:relative}
#core-wraper .admin-main .left-nav-list-bg{background: none repeat scroll 0 0 #f2f2f2;height: 100%;left: 0;position: absolute;top: -5px;width: 176px;z-index:0; border-right:1px solid #E8E8E8;}

.box{
	margin:0px 0px 12px;
	background-color:#FEFEFE; 
	border-width:1px;
	border-style:solid;
	border-color:#CCC;
	box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.15); 
	border-radius:6px;
	}
.section-header{
	overflow: hidden;
    min-height: 22px;
	margin:0px 0 10px 0;
	}
.section-header h2{

	background:/*-moz-linear-gradient(center top , #E0E0E0, #CCC) repeat scroll 0 0 #BBBBBB*/ -moz-linear-gradient(center top , #FCFCFC, #F1F1F1) repeat scroll 0 0 #F7F7F7;
	box-shadow:0 1px 2px rgba(0, 0, 0, 0.2);
	border-radius:6px 6px 0 0;
	padding:8px 15px;
	color: #555555;
    font-size: 14px;
    font-weight: bold;
    padding: 8px 15px;
    text-shadow: 0 1px 0 #F2F2F2;
    text-transform: uppercase;	
	}
.quick-actions-toolbar{border-bottom:1px solid #EEE;border-top:1px solid #C1C1C1;padding:2px 15px;background:none repeat 0 0 rgba(0,0,0,0.01);}
.action_sets.left{float:left;}
.action_sets.right{float:right;}
.quick-actions-toolbar .action_sets li{
    float: left;
    line-height: 1;
    margin:0;	
	font-size:11px;
	color:#999999;
}
.quick-actions-toolbar .action_sets .command {
	padding:7px 6px 6px 0;
	display:block;
	color:#333;
}
.quick-actions-toolbar .action_sets li a{
    display: block;
    padding: 6px 16px 6px 6px;
	position:relative;
	z-index:4;
	border:1px solid transparent;
	}
.quick-actions-toolbar .action_sets li a:hover{
    border-radius: 5px 5px 0px 0px;
    color: brown;
	}
.quick-actions-toolbar .action_sets li a .arrow {
    border-color: #C1C1C1 transparent transparent;
    border-style: solid;
    border-width: 3px;
    height: 0;
    position: absolute;
    right: 6px;
    top: 11px;
    width: 0;
}	
.action_sets li:hover a.dropdown-tab{	
   	background: none repeat-x scroll 0 50% #FFF;
	border-radius: 5px 5px 0px 0px;
	color:brown;
	box-shadow: 2px 0px 0px rgba(0, 0, 0, 0.1);	
	border-width:1px 1px 0 1px;
	border-style:solid;
	border-color:#A8A8A8 #AAA #AAA #AAA;
	padding-bottom:7px;	
}
.action_sets li.settings a.dropdown-tab{
	background-image: url(../images/icons/b/19.png); 
	background-repeat:no-repeat; 
	background-position:50%; 
	text-indent:-9999em;
}
.action_sets li ul.dropdown-list {
    background: none repeat scroll 0 0 #FFF;
    border-radius: 0 5px 5px 5px;
    box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.2);
    display: none;
    float: left;
    padding: 5px 0;
	margin-top:-1px;
    position: absolute;
    width: 120px;
    z-index: 3;
	border-width:1px 1px 1px;
	border-style:solid;
	border-color:#AAA #AAA #9A9A9A #AAA;
}
.action_sets li ul.dropdown-list li {
    clear: left;
    margin: 0;
    width: 100%;
}
.action_sets li ul.dropdown-list li strong{
	padding:10px;
	display:block;
	color:#999; 
	border-bottom:1px solid #EEE;	
}
.action_sets li ul.dropdown-list li a{
    background: none repeat scroll 0 0 transparent;
    box-shadow: none;
    color: #222222;
    font-size: 11px;
    font-weight: normal;
    padding: 5px 10px;
	border:none;
}
.action_sets li ul.dropdown-list li a:hover{
    background: none repeat scroll 0 0 #454545;
    border-radius: 0 0 0 0;
	border:none;
    color: #FFF;
}
	
.icon-list{
	-moz-transition: all 0.4s ease-in-out 0s;
	-webkit-transition: all; 
	-webkit-transition-duration : 0.4s; 
	-webkit-transition-timing-function : ease-in-out;
	margin:10px;
	}	
.icon-list ul {
    overflow: hidden;
    padding:0;
	margin-bottom:0;
}
.icon-list ul li {display:inline}
.icon-list ul li a {
	background-color:#EEE;	
	border-radius:4px;
	margin:5px;
	padding:5px;
	float:left;
	color: #444;
    display: block;
    height: 60px;
	width:59px;
    text-align: center;
    text-decoration: none;
	-moz-transition: all 0.2s ease-in-out 0s;
	-webkit-transition-property : all; 
	-webkit-transition-duration : 0.2s; 
	-webkit-transition-timing-function : ease-in-out;
}
.icon-list ul li.not-used a, .icon-list ul li#page-custom a{border:1px dashed #DDD; padding:4px; background-color:#FFF;opacity: 0.3;}
.icon-list ul li a:hover{background-color:#EEF3FB;}
.icon-list ul li .icon {
	background-color:transparent;
	background-repeat:no-repeat;
	background-position:0 0;
    display: block;
    height: 24px;
    width: 24px;
    margin: 10px auto 0;
	opacity:.7;
	background-image:url(../images/icons/icon24-sp.png);
}
.icon-list ul li a:hover .icon{opacity:1 !important;}
.icon-list ul li .title {
