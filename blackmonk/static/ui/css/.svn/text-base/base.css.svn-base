/* -------- Alerts ------- */

.info-bar {font-size: 11px; padding: 5px 20px 8px; color: rgb(255, 255, 255); background: none repeat scroll 0pt 0pt rgb(134, 159, 209); border: 1px solid rgb(103, 131, 187);}
.info-bar a{color:#FFF; text-decoration:underline;}

.alert-bar {
	background: none repeat scroll 0 0 #333333;
    border: medium none;
    border-radius: 5px 5px 5px 5px;
    box-shadow: 1px 2px 3px rgba(0, 0, 0, 0.2);
    margin: 0;
    min-height: 20px;
	min-width:360px;
    opacity: 0.95;
    padding: 25px 20px;
    position: fixed;
    right: 10px;
    top: 63px;
    z-index: 999;	
	opacity:0;
	-webkit-transform: scale(0.85);
	   -moz-transform: scale(0.85);
		-ms-transform: scale(0.85);
		 -o-transform: scale(0.85);
	-webkit-transition:all 0.2s ease-in 0s;
	   -moz-transition:all 0.15s ease-in 0s;
		-ms-transition:all 0.2s ease-in 0s;
		 -o-transition:all 0.2s ease-in 0s;
}
.alert-bar.active{
	-webkit-transform: scale(1);
	   -moz-transform: scale(1);
		-ms-transform: scale(1);
		 -o-transform: scale(1);
	-webkit-transition:all 0.2s ease-in 0s;
	   -moz-transition:all 0.15s ease-in 0s;
		-ms-transition:all 0.2s ease-in 0s;
		 -o-transition:all 0.2s ease-in 0s;
	opacity:1;
}
.alert-bar .fit_width{
	width:auto;
}
.alert-bar .icon-alert {
    display: inline-block;
    height: 24px;
    margin-left: 5px;
    margin-right: 5px;
    vertical-align: middle;
    width: 24px;
}
.alert-success .icon-alert {
    background:url(../images/global/success.png) no-repeat scroll 0 0 transparent;
}
.alert-error .icon-alert {
    background:url(../images/global/error-icon.png) no-repeat scroll 0 0 transparent;
}
.alert-warning .icon-alert {
    background:url(../images/global/warning.png) no-repeat scroll 0 0 transparent;
}
.alert-bar h4 {
    color: #FAFAFA;
    display: inline-block;
    font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
    font-size: 14px;
    font-weight: normal;
    line-height: 20px;
    padding: 0;
    text-shadow: none;
    vertical-align: middle;
}

