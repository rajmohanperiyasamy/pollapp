/* Tipsy */

.tipsy { padding: 5px; font-size: 11px;font-weight:normal; position: absolute; z-index: 99999; }
.tipsy-inner { padding: 3px 8px 4px 8px;background:transparent;zoom: 1; background:none repeat 0 0 scroll black;color: white; text-shadow:0 1px 0 #000; max-width: 200px; text-align: center; }
.tipsy-inner {
	-webkit-box-shadow:0 1px 0 rgba(0,0,0, 0.12);
	   -moz-box-shadow:0 1px 0 rgba(0,0,0, 0.12);
		 -o-box-shadow:0 1px 0 rgba(0,0,0, 0.12);
			box-shadow:0 1px 0 rgba(0,0,0, 0.12);
}
.tipsy-arrow { position: absolute; background: url("../images/global/121211bm002.gif") no-repeat top left; width: 9px; height: 5px;z-index:100000 }
.ftip .tipsy-arrow { 
	background:url(../images/icons/e/vtab-arrow.png) no-repeat top left; 
	height: 6px; 
	width: 15px;
}
.tipsy-n .tipsy-arrow { top: 0px; left: 50%; margin-left: -4px; }
.tipsy-nw .tipsy-arrow { top: 0; left: 10px; }
.tipsy-ne .tipsy-arrow { top: 0; right: 10px; }
.tipsy-s .tipsy-arrow { bottom: 0; left: 50%; margin-left: -4px; background-position: bottom left; }
