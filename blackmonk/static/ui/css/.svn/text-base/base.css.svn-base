/* Reset styles
---------------------------------------------- */

html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp, small, strong, sub, sup, var, 
b, i, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, figcaption, figure, footer, header, hgroup, menu, nav, section, summary, time, mark, audio, video, xmp {
	margin:0;
	padding:0;
	border:0;
	font-size:100%;
	font:inherit;
	vertical-align:baseline;
	background:transparent;
}
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {display:block}
ol,  ul {list-style: none;}
blockquote, q {quotes:none}
blockquote:before, blockquote:after, q:before, q:after {content:'';content:none}
table {border-collapse: collapse;border-spacing: 0;}


/* Base styles
---------------------------------------------- */

body {
	font:normal 12px/150% 'Helvetica Neue',Helvetica,Arial,sans-serif; 
	color:#232323;
	background:none #FFFFFF;
}
select{height:30px;}
select[multiple] {
    height: auto;
}
/* --------- links ----------- */

a:link, a:visited {color: #4083a9;outline: none; text-decoration: none;}
a:hover {text-decoration: none;color: #205f82;}

a.read-more {
    background: none no-repeat scroll 100% 55% transparent;
    display: none;
    float: right;
    outline: medium none;
    padding-right: 12px;
}                                                   
a.read-more {background-image:url(../images/global/read-more.png);}
a.read-more.up {background-image:url(../images/global/read-more-up.png);}

/* -------- Typography ------- */

h1{font-size:24px;font-weight:normal;color:#333}
h2{font-size:20px;font-weight:bold;color:#333}
h3{font-size:18px;font-weight:bold;color:#333}
h4{font-size:14px;font-weight:normal;color:#333}
h5{font-size:14px;font-weight:bold;;color:#333}
h6{font-size:14px;font-weight:normal;color:#333}
p{margin-bottom:1em;line-height:1.5em}
.txt-right{text-align:right}
.txt-center{text-align:center}
.txt-bold{font-weight:bold}
.txt-italic{font-style:italic}
.txt-underline{text-decoration:underline}
.txt-regular{font-weight:normal}
a.small{font-size:11px;}

/* -------- Misc ------- */

.mrl0{margin-left:0px !important;}
.mrb0{margin-bottom:0px !important;}
.pd50{padding:5px 0 !important;}
.pdr10{ padding-right:10px;}
.mat25{ margin-top:25px !important;}
.pdr6{padding-right:6px !important;}
.pdt0{padding-top:0 !important;}


/* -------- Site Bar ------- */

.account-bar{
	position: fixed;
	top: 0;
