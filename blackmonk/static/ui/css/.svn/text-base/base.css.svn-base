.bm-selectmenu {
	display: block; 
	position:relative; 
	height:28px; 
	text-decoration: none; 
	overflow:hidden;
	background:url(../images/ie-fix/selectmenu.png);
	background-color: #f4f4f4;
	background-repeat: repeat-x;
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FEFEFE), to(#EEE));
	background: -webkit-linear-gradient(top , #FEFEFE, #EEE);
	background: -moz-linear-gradient(center top , #FEFEFE, #EEE);
	background: -ms-linear-gradient(top , #FEFEFE, #EEE);
	background: -o-linear-gradient(top , #FEFEFE, #EEE);
	border-radius: 3px;
	border-width: 1px; 
	border-style:solid;
	border-color:#C2C2C2;
	color: #EEEEEE;
	font-weight: bold; 
	outline: medium none;
	font-size:12px;
	padding:0px;
}
.bm-selectmenu-icon { position:absolute; right:0px; top:0; background:url(../images/global/eklr2389.png) no-repeat scroll 50% 50% rgba(0,0,0,0.05);height:100%;width: 22px;display: block;overflow: hidden;text-indent: -99999px; }
.bm-selectmenu-menu { padding:0; margin:0; list-style:none; position:absolute; top: 0; visibility: hidden; overflow: auto; border:1px solid #bbb; }
.bm-selectmenu-open { visibility: visible; z-index:99999; }
.bm-selectmenu-menu li { padding:0; margin:0; display: block; border:none; font-weight: normal !important; }
.bm-selectmenu-menu li:hover{ 
	border:none;
	background:none repeat 0 0 #F2F2F2;
	box-shadow:none;
}
.bm-selectmenu-menu li:hover a{color:#000;}
.bm-selectmenu-menu li a,.bm-selectmenu-status {line-height: 18px; display:block; padding:5px 10px 7px; outline:none; text-decoration:none;}
.bm-selectmenu-menu li a{color:#666}
.bm-selectmenu-status { line-height: 16px;color:#333; }
.bm-selectmenu-menu-items {
	background: none repeat scroll 0 0 #FFF;
	border-bottom: 1px solid #C2C2C2;
	-webkit-box-shadow: 0 0 3px rgba(0, 0, 0, 0.2);
	   -moz-box-shadow: 0 0 3px rgba(0, 0, 0, 0.2);
		 -o-box-shadow: 0 0 3px rgba(0, 0, 0, 0.2);
			box-shadow: 0 0 3px rgba(0, 0, 0, 0.2);
	font-size: 1em;
}
.bm-selectmenu:hover{ 
	border: 1px solid #BBB;
	background:url(../images/ie-fix/selectmenu-hover.png);
	background-color: #F8F8F8;
	background-repeat: repeat-x;
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#F9F9F9), to(#E9E9E9));
	background: -webkit-linear-gradient(top , #F9F9F9, #E9E9E9);
	background: -moz-linear-gradient(center top , #F9F9F9, #E9E9E9);
	background: -ms-linear-gradient(top , #F9F9F9, #E9E9E9);
	background: -o-linear-gradient(top , #F9F9F9, #E9E9E9);
	color: #333333;
	-webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
	   -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
		 -o-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
			box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
}
.bm-selectmenu.state-active { 
	-webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1) inset;
	   -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1) inset;
		 -o-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1) inset;
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1) inset;
	border-color:#AAAAAA;
}

.bm-selectmenu-disabled{ opacity:0.35; cursor:default;}
.bm-selectmenu-focus{
	border: 1px solid #5393FC; 
	-webkit-box-shadow:0 0 2px rgba(77, 144, 254, 0.65); 
	   -moz-box-shadow:0 0 2px rgba(77, 144, 254, 0.65); 
		 -o-box-shadow:0 0 2px rgba(77, 144, 254, 0.65); 
			box-shadow:0 0 2px rgba(77, 144, 254, 0.65); 
	outline: medium none ;
}

.bm-selectmenu-menu .bm-selectmenu-group {font-size: 1em;}
.bm-selectmenu-menu .bm-selectmenu-group:hover{ border:none;background:none;box-shadow:none;}
.bm-selectmenu-menu .bm-selectmenu-group:hover a{color:#666;}
.bm-selectmenu-menu .bm-selectmenu-group .bm-selectmenu-group-label {display: block;font-weight: normal;line-height:22px;padding: 5px 10px;color:#CCC; border-top:1px solid #e6e6e6}
.bm-selectmenu-menu .bm-selectmenu-group ul {margin: 0;padding: 0;}

.bm-selectmenu-menu li.parent a { font-weight:bold; color:#222; }
.bm-selectmenu-menu li.child a {padding-left:20px; }


