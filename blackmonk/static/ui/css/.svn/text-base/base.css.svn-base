	z-index: 10000 !important;
    opacity: 1;
    filter:alpha(opacity=100);
	visibility:visible;
  }
.account-bar .primary-nav ul li:hover .sub-menu li{
    height: 20px;
}
.account-bar .primary-nav ul li .sub-menu {
    background:none repeat 0 0 scroll rgba(0,0,0,0.85);
	border:1px solid rgba(0,0,0,1);
	-webkit-box-shadow:0 0px 3px rgba(0,0,0, 0.6); 
	   -moz-box-shadow:0 0px 3px rgba(0,0,0, 0.6); 
		 -o-box-shadow:0 0px 3px rgba(0,0,0, 0.6); 
			box-shadow:0 0px 3px rgba(0,0,0, 0.6); 
	visibility:hidden;
    opacity: 0;
	z-index: -1;
    filter:alpha(opacity=0);
	white-space: nowrap;
	border-radius:3px;
    height: auto;
    left: 0;
    margin: 0;
    padding:0;
    position: absolute;
    top: 29px;
    width: 200px;
