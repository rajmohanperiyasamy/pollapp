	background: none repeat scroll 0 0 #517DC5;
    border-bottom: 1px solid rgba(0, 0, 0, .45);
	   -moz-box-shadow: 0 0px 3px rgba(0, 0, 0, 0.5);
	-webkit-box-shadow: 0 0px 3px rgba(0, 0, 0, 0.5);
		 -o-box-shadow: 0 0px 3px rgba(0, 0, 0, 0.5);
			box-shadow: 0 0px 3px rgba(0, 0, 0, 0.5);
	height: 52px;
	opacity: 1;
	filter: alpha(opacity=100);
	z-index: 990;
	}
#site-name {margin:0 15px;float:left}	
#site-name h1.account-logo {
    font:bold 24px/175% 'Helvetica',Helvetica,Arial,sans-serif;
	letter-spacing:-1px;
	text-shadow:0px -1px 0 rgba(0,0,0,.2)
}
#site-name h1.account-logo a{
	color:rgba(255,255,255,.975);
	transition: opacity 0.15s ease 0s;
	-webkit-transition: opacity 0.15s ease 0s;
