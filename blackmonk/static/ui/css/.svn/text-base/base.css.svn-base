.bm-menu .bm-menu-item {
	margin:0;
	padding: 0;
	zoom: 1;
	float: left;
	clear: left;
	width: 100%;
}
.bm-menu .bm-menu-item a {
	text-decoration:none;
	display:block;
	padding:5px 10px 7px;
	line-height:18px;
	zoom:1;
	color:#666;
}
.bm-menu .bm-menu-item a.bm-state-hover,
.ubm-menu .bm-menu-item a.bm-state-active {
	background:#f2f2f2;
	cursor:pointer;
}

