	-webkit-transition:opacity 0.2s ease-out 0s;
	   -moz-transition:opacity 0.2s ease-out 0s;
	    -ms-transition:opacity 0.2s ease-out 0s;
		 -o-transition:opacity 0.2s ease-out 0s;
			transition:opacity 0.2s ease-out 0s;
}
.tipsy.tour .tour_extra{
	bottom: 20px;
    left: 20px;
    position: absolute;
    width: 380px;
	height:24px;
}
.tipsy.tour .button_cont {
    display:block !important;
    float:left;
}
.tipsy.tour button{
    background: none repeat scroll 0 0 #EEEEEE;
    border: 1px solid #FAFAFA;
    border-radius: 2px 2px 2px 2px;
    color: #333333;
    cursor: pointer;
    display: inline-block;
    font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
    font-size: 12px;
    font-weight: bold;
    line-height: 17px;
    margin-right: 5px;
    padding: 2px 10px;
}
.tipsy.tour button:disabled{
    color: #999999;
    pointer-events: none;
}
.tipsy.tour button.next:hover, .tipsy.tour button.prev:hover {
    background: #fafafa;
}
.tipsy.tour .tour_nav_cont {
    float: right;
    margin-top: 9px;
}
.tipsy.tour .tour_nav_cont li {
    background: none repeat scroll 0 0 #555555;
    border-radius: 50% 50% 50% 50%;
    float: left;
    margin-left: 3px;
    padding: 3.4px;
}
.tipsy.tour .tour_nav_cont li.active{
	background: none repeat scroll 0 0 #999;
}
.tour .tipsy-inner .tip-loader {
    height: 200px;
    text-align: center;
    width: 100%;
}
.tipsy.tour .tipsy-inner {
	height:205px;
    background: none repeat scroll 0 0 #333;
    border: 1px solid black;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.5);
    width: 380px;
	max-width: 380px;
    padding: 20px 20px 60px;
    text-align: left;
	color:#fafafa;
		 	transition: all 0.2s ease-in 0s;
	   -moz-transition: all 0.2s ease-in 0s;
	-webkit-transition: all 0.2s ease-in 0s;
		-ms-transition: all 0.2s ease-in 0s;
		 -o-transition: all 0.2s ease-in 0s;
}
.tour_link li{
	height:0;
	width:0;
	padding:0;
}
.tour_content{
    -webkit-user-select: none;
     -khtml-user-select: none;
       -moz-user-select: none;
    	 -o-user-select: none;
  			user-select: none;
}
.tour_content h1 {
    color: #FAFAFA;
    font-size: 19px;
    font-weight: bold;
    margin-bottom: 20px;
}
.tour_content p {
    font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
    font-size: 14px;
}
.tour_content ul {
    list-style: disc inside none;
}
.tour_content ul li {
    font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif;
    font-size: 12px;
    line-height: 22px;
}
.account-bar nav.quick_nav a.tour{
	display:none;
}

#site_loader {
    background: url("../images/icons/ajax-loader.gif") no-repeat scroll 0 0 transparent;
    display: block;
    height: 28px;
    left: 50%;
    position: absolute;
    text-indent: -999em;
    top: 13px;
    width: 28px;
}

.alert-bar .icon-close {
    bottom: 22px;
    color: #CCCCCC;
    cursor: pointer;
    font-size: 18px;
    font-weight: bold;
    position: absolute;
    right: 10px;
    top: 5px;
    -moz-transition:color 0.35s 0s;
    -webkit-transition:color 0.35s 0s;
    -o-transition:color 0.35s 0s;
    -ms-transition:color 0.35s 0s;
}
.alert-bar .icon-close:hover{
	color:#FFF;
}

