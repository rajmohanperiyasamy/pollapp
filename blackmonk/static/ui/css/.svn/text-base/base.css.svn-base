input[type="email"], input[type="number"], input[type="password"], input[type="text"], input[type="url"], input[type="search"] {
    background: none repeat scroll 0 0 #FFFFFF;
	-webkit-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
	   -moz-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
		 -o-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
			box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
    border-color: #BBB #C2C2C2 #C9C9C9;
    border-radius: 2px 2px 2px 2px;
    border-style: solid;
    border-width: 1px;
    display: inline-block;
    height: 26px;
	line-height:27px;
    margin:0;
    padding-left: 6px;
	font: 13px/16px Arial, sans-serif;
}
input[type="text"] {
  line-height: 30px\0/;/*only for ie 8*/
}
*+html input[type="text"] {
	line-height:30px;
}
input[type="email"]:hover, input[type="number"]:hover, input[type="password"]:hover, input[type="text"]:hover, input[type="url"]:hover, input[type="search"]:hover {
    border-color: #A0A0A0 #B2B2B2 #B9B9B9;
    border-style: solid;
    border-width: 1px;
    -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25) inset;
	   -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25) inset;
		 -o-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25) inset;
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25) inset;
}
input[type="email"][disabled="disabled"], input[type="number"][disabled="disabled"], input[type="password"][disabled="disabled"], input[type="text"][disabled="disabled"], input[type="url"][disabled="disabled"] {
    background: none repeat scroll 0 0 #EAEAEA;
}
input[type="email"][disabled="disabled"]:hover, input[type="number"][disabled="disabled"]:hover, input[type="password"][disabled="disabled"]:hover, input[type="text"][disabled="disabled"]:hover, input[type="url"][disabled="disabled"]:hover {
    -webkit-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
	   -moz-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
		 -o-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
			box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
    border-color: #BBB #C2C2C2 #C9C9C9;
    border-style: solid;
    border-width: 1px;
}
input[type="email"]:focus, input[type="number"]:focus, input[type="password"]:focus, input[type="text"]:focus, input[type="url"]:focus, input[type="search"]:focus {
    border: 1px solid #5393fc;
    -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset, 0px 0px 2px rgba(77,144,254, 0.65);
	   -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset, 0px 0px 2px rgba(77,144,254, 0.65);
		 -o-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset, 0px 0px 2px rgba(77,144,254, 0.65);
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset, 0px 0px 2px rgba(77,144,254, 0.65);
    outline: medium none;
}
textarea.textField, textarea {
    background: none repeat scroll 0 0 #FFFFFF;
	-webkit-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
	   -moz-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
		 -o-box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
			box-shadow:inset 0px 1px 1px rgba(0, 0, 0, 0.1);
    border-color: #BBB #C2C2C2 #C9C9C9;
    border-radius: 2px 2px 2px 2px;
    border-style: solid;
    border-width: 1px;
    margin: 0;
    padding: 6px;
	font: 13px/16px Arial, sans-serif;
}
textarea.textField:hover, textarea:hover {
    border-color: #A0A0A0 #B2B2B2 #B9B9B9;
    border-style: solid;
    border-width: 1px;
    -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25) inset;
	   -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25) inset;
		 -o-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25) inset;
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.25) inset;
}
textarea.textField:focus, textarea:focus {
    border: 1px solid #5393fc;
    -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset, 0px 0px 2px rgba(77,144,254, 0.65);
	   -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset, 0px 0px 2px rgba(77,144,254, 0.65);
		 -o-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset, 0px 0px 2px rgba(77,144,254, 0.65);
			box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset, 0px 0px 2px rgba(77,144,254, 0.65);
