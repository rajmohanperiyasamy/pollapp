.section-header .tabs{background: none repeat scroll 0 0 #FDFDFD;border-bottom: 1px solid #EEE;border-top: 1px solid #D2D2D2;padding:0 15px;box-shadow:0px 1px 2px rgba(0,0,0,0.07) inset/*, 0px 1px 1px rgba(0,0,0,0.04)*/}
.section-header .tabs ul li{float:left;}
.section-header .tabs ul li a{float:left; padding:6px 15px;font-size:11px;color:#888;/*background-color:rgba(0,0,0,0.01);*/box-shadow:1px 0 0 0 rgba(0,0,0,0.05) inset; font-weight:bold; text-shadow:0 1px 0 #FFF}
