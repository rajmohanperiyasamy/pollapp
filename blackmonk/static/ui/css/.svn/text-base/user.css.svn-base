	margin:0px 0px 12px;
	background-color:#FEFEFE; 
	border-width:1px;
	border-style:solid;
	border-color:#CCC;
	box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.15); 
	border-radius:6px;
	}
.section-header{
	overflow: hidden;
	min-height: 22px;
	margin:0px 0 10px 0;
}
